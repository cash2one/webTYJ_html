    <style>
        .border{
            border: 1px solid #e4e4e4;
            /*width:200px;*/
            /*height: 480px;*/
            /*overflow: scroll;*/
            /*overflow-x: scroll;*/
        }
        .borderTB{
            border-top: 1px solid #e4e4e4;
            border-bottom: 1px solid #e4e4e4;
        }
        .borderInfo{
            height: 480px;
            border-bottom: 1px solid #e4e4e4;
