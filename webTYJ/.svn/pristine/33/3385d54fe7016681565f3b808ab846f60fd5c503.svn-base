                        <h5 ng-switch-when="1">处理</h5>
                        <h5 ng-switch-when="2">验收</h5>
                        <h5 ng-switch-when="3">检测</h5>
                        <h5 ng-switch-when="4">结算</h5>
                        <h5 ng-switch-when="5">赔偿给业主</h5>
                        <h5 ng-switch-when="17">表扬</h5>
                        <h5 ng-switch-when="6">部门质检</h5>
                        <h5 ng-switch-when="8">申诉</h5>
                        <h5 ng-switch-when="7">回访</h5>
                        <h5 ng-switch-when="9">查抄水表</h5>
                        <h5 ng-switch-when="10">查抄电表</h5>
                        <h5 ng-switch-when="11">投诉</h5>
                        <h5 ng-switch-when="12">咨询</h5>
                        <h5 ng-switch-when="22">维修</h5>
                        <h5 ng-switch-when="23">向业主索赔</h5>
                        <h5 ng-switch-when="24">失效</h5>
                        <h5 ng-switch-when="13">固定车位</h5>
                    </td>
                    <td align="center" valign="middle" width="50"><span style="margin-left: 0" class="line_blue"></span></td>
                    <td align="center" valign="middle" width="25"  ><a ng-click="item1(item)" ng-class="isActive==item.tasksJournalId?'back_yellow':'back_blue'" class="radius_25 icon" ></a></td>
                    <td align="center" valign="middle" width="50"><span style="margin-left: 0" class="line_blue"></span></td>
                    <td style="width: 135px; text-align: center">
                        {{item.operationTime }}
                    </td>
                    <td width="50" align="center">&nbsp;</td>
                    <td width="140">&nbsp;</td>
                    <td>&nbsp;</td>
                    <td width="50" align="center">&nbsp;</td>
                    <td width="140">&nbsp;</td>
                    <td >&nbsp;</td>
                    <td  style=" text-align: center;width: 135px;"  >
                <!--        <table  class="table_lists" width="150" border="0" cellspacing="0" cellpadding="0" id="tableTime">
                            <tr class="progress_bar">
                                <td align="center" valign="middle" width="50"><span style="margin-left: 0" class="line_blue"></span></td>
                                <td align="center" valign="middle" width="25"  ><a ng-click="item1(item)" ng-class="isActive==item.tasksJournalId?'back_yellow':'back_blue'" class="radius_25 icon" ></a></td>
                                <td align="center" valign="middle" width="50"><span style="margin-left: 0" class="line_blue"></span></td>
                            </tr>
                        </table>-->
                    </td>
                    <td width="140">&nbsp;</td>
                    <td >&nbsp;</td>
                </tr>
             <!--   <tr>
                    <td ng-repeat="item in TasksJournal" style=" text-align: center;width: 135px;"  >
                        <table  class="table_lists" width="150" border="0" cellspacing="0" cellpadding="0" id="tableTime">
                            <tr class="progress_bar">
                                <td align="center" valign="middle" width="50"><span style="margin-left: 0" class="line_blue"></span></td>
                                <td align="center" valign="middle" width="25"  ><a ng-click="item1(item)" ng-class="isActive==item.tasksJournalId?'back_yellow':'back_blue'" class="radius_25 icon" ></a></td>
                                <td align="center" valign="middle" width="50"><span style="margin-left: 0" class="line_blue"></span></td>
                            </tr>
                        </table>
                    </td>
                    <td width="140">&nbsp;</td>
                    <td >&nbsp;</td>
                </tr>-->
              <!--  <tr >
                    <td style="width: 135px; text-align: center" ng-repeat="item in TasksJournal">
                        {{item.operationTime }}
                    </td>
                    <td width="50" align="center">&nbsp;</td>
                    <td width="140">&nbsp;</td>
                    <td>&nbsp;</td>
                </tr>-->
            </table>
        </div>
        <!--<div class="progress_bar   overflow_x">-->
        <!--<a href="#" class="radius_25  back_blue  boder_white  icon"></a>-->
        <!--<span class="line_blue"></span>-->
        <!--<a href="#" class="radius_25  back_blue  boder_white  icon"></a>-->
        <!--<span class="line_grey"></span>-->
        <!--</div>-->
<!--处理进度 end-->
<div class="row search-padding-10" ng-show="taskShow">
    <div class="new_order_left ">
        <button type="button" ng-click="addTask1()" class="btn btn-default btn-primary" >
            <span class="glyphicon glyphicon-plus btn_icon"></span>
            新建工单
        </button>

    </div>
    <div  ng-show="order"  class="col-md-1 text-right">
        <span>工单类型</span>
    </div>
    <div  ng-show="order"  class="col-md-3">
        <select  id="s" class="form-control" ng-model="orderItem" ng-change="orderChange(orderItem)">
            <option value=""   >请选择工单类型</option>
            <option name="option2" id="option2" value="报价单" >报价工单</option>
            <option name="option3" id="option3" value="检测" >检测工单</option>
            <option name="option4" id="option4" value="处理" >处理工单</option>
            <option name="option5" id="option5" value="验收" >验收工单</option>
            <option name="option6" id="option6" value="赔偿给业主" >赔偿给业主</option>
            <option name="option7" id="option7" value="表扬" >表扬工单</option>
            <option name="option8" id="option8" value="结算">结算工单</option>
            <option name="option9" id="option9" value="回访">回访工单</option>
            <option name="option10" id="option10" value="验房">验房工单</option>
            <option name="option11" id="option11" value="查抄水表">查抄水表</option>
            <option name="option16" id="option16" value="查抄电表">查抄电表</option>
            <option name="option12" id="option12" value="申诉工单">申诉工单</option>
            <option name="option13" id="option13" value="投诉工单">投诉工单</option>
            <option name="option14" id="option14" value="咨询工单">咨询工单</option>
            <option name="option15" id="option15" value="固定车位工单">固定车位</option>
            <option name="option17" id="option17" value="维修">维修工单</option>
            <option name="option18" id="option18" value="失效">失效工单</option>
            <option name="option19" id="option19" value="向业主索赔">向业主索赔</option>
        </select>
    </div>


<div id="content-first"  ng-show="order1" class="complaint_left">
    <!--新建报价单开始-->
    <div id="div1">

        <div id="tab">
            <a lang="no" class="this_action">报价单</a>
        </div>
        <div class="row padding">
            <div class="col-lg-2 col-md-2 col-sm-12"></div>
            <div class="col-lg-8 col-md-8 col-sm-12">
                <div class="search-padding-10">
                    <button type="button" class="btn btn-default btn-primary" ng-click="deleteQuotation()" >
                        <span class="glyphicon glyphicon-remove btn_icon"></span>
                        删除
                    </button>
                    <button type="button" class="btn btn-default btn-primary"  ng-click="addbao()" >
                        <span class="glyphicon glyphicon-plus btn_icon"></span>
                        新建
                    </button>
                </div>
                <table>
                    <thead>
                    <tr>
                        <th><input ng-show="itemRecords.length!=0" type="checkbox" id="allChoose2" ng-click="allChoose('allChoose2','quoteId')"/></th>
                        <th>类型</th>
                        <th>名称</th>
                        <th width="17%">单价</th>
                        <th width="17%">数量</th>
                        <th>总额（元）</th>
                    </tr>
                    </thead>
                    <tbody>
                    <tr ng-repeat="item in itemRecords">
                        <td><input type="checkbox" name="quoteId" ng-click="getChoose(item.id,$event,'allChoose2','quoteId')"  id="{{item.id}}"/></td>
                        <td>
                            <select class="form-control" ng-model="item.quoteItemType">
                                <option value="">- -请选择- -</option>
                                <option value="0">人工费</option>
                                <option value="1">物料费</option>
                                <option value="2">工具费</option>
                            </select>
                        </td>
                        <td>
                            <select class="form-control" ng-model="item.quoteItemName">
                                <option value="">- -请选择- -</option>
                                <option value="电工">电工</option>
                                <option value="灯管">灯管</option>
                                <option value="绝缘梯">绝缘梯</option>
                            </select>
                        </td>
                        <td><input class="form-control"  ng-model="item.quoteItemPrice" ng-blur="SumOrUpdateQuoteOrders()"></td>
                        <td><input class="form-control" ng-model="item.quoteItemNum" ng-blur="SumOrUpdateQuoteOrders()"></td>
                        <td ng-if="item.quoteItemPrice!=null&&item.quoteItemNum!=null">
                            <input type="hidden" value="{{item.quoteItemPrice*item.quoteItemNum}}" ng-model="item.quoteItemTotal" />
                            {{item.quoteItemPrice*item.quoteItemNum}}元
                        </td>
                        <td ng-if="item.quoteItemPrice==null||item.quoteItemNum==null">0.0元</td>
                    </tr>
                    </tbody>
                </table>
                <div class="sel1 tab_info">
                    <div class="row">
                        <div class="col-lg-2 col-md-2 col-sm-2 text-right">总额:</div>
                        <div class="col-lg-4 col-md-4 col-sm-4">{{QuoteOrders.allTotal}}元</div>
                        <div class="col-lg-2 col-md-2 col-sm-2 text-right">折扣</div>
                        <div class="col-lg-4 col-md-4 col-sm-4"><input class="form-control" ng-model="QuoteOrders.discount" ng-blur="discountOrUpdateQuoteOrders()"></div>
                        <div class="col-lg-2 col-md-2 col-sm-2 text-right">最终价格</div>
                        <div class="col-lg-4 col-md-4 col-sm-4"><input class="form-control" value="itemRecordsDiscount" ng-model="QuoteOrders.finalPrice"></div>
                        <div class="col-lg-2 col-md-2 col-sm-2 text-right">签字人</div>
                        <div class="col-lg-4 col-md-4 col-sm-4"><input class="form-control"  ng-model="QuoteOrders.signature" placeholder="默认为业主" data-toggle="modal" data-target="#myModalPerson" readonly="readonly"></div>
                        <div class="col-lg-2 col-md-2 col-sm-2  text-right">
                            附件列表
                        </div>
                        <div class="col-lg-10 col-md-10 col-sm-10 text_right">
                            <button class="btn btn-default btn-primary pull-right" role="presentation" data-toggle="modal" data-target="#ddd" style="cursor:pointer" tabindex="-1" data-toggle="tab" ng-click="quoteOrdersfile()">上传</button>
                        </div>
                        <div class="col-lg-10 col-md-10 col-sm-12 col-lg-offset-2 col-md-offset-2 text-left">
                            <span ng-if="annexs != null">
                                <span class="text-center" ng-repeat="item in QuoteOrders.annexs" style="display:inline-block;margin-right:5px;">
                                    <img src={{fileUrl}}/{{item.annexAddress}} style="display:block;margin-bottom:5px;width: 100px;height: 100px"><button class="btn btn-default btn-primary tab-btn" ng-click="delPic1(item);">移除</button>
                                </span>
                            </span>
                        </div>

                    </div>
                </div>
                <div class="sel1 text_center">
                    <button type="button" class="btn btn-default btn-primary" ng-click="colseConsultation(1)" >
                        取消
                    </button>
                    <button type="button" class="btn btn-default btn-primary" ng-click="AddOrUpdateQuoteOrders(QuoteOrders.taskId)" >
                        提交
                    </button>
                </div>

            </div>
        </div>
    </div>
    <!--  失效单-->
    <div id="div17"  style="display:none;">
        <!--tab页切换开始-->
        <div id="tab">
            <a lang="no" class="this_action">任务失效单</a>
        </div>
        <div class="row">
            <div class=" col-lg-2"></div>
            <div class=" col-lg-8">
                <div class="row sel1 tab_info">
                    <div class="col-lg-2 col-md-2 col-sm-2 text-right">失效说明：<span style="color: red">*</span></div>
                    <div class="col-lg-9 col-md-10 col-sm-10">
                        <textarea class="text_min" ng-model="ClosedOrders.remarks"></textarea>
                    </div>
                </div>
            </div>
            <div class="clear"></div>
            <div class="sel1 text_center">
                <button type="button" class="btn btn-default btn-primary" ng-click="colseConsultation(14)">
                    取消
                </button>
                <button type="button" class="btn btn-default btn-primary" ng-click="updateClosed(ClosedOrders.taskId)" >
                    提交
                </button>
            </div>
        </div>
    </div>
    <!--  失效单-->
    <!-- 检测单 -->
    <div id="div2" style="display:none;">
        <div id="tab">
            <a lang="no" class="this_action">检测单</a>
        </div>
        <div class="row padding">
            <div class="col-lg-2"></div>
            <div class="col-lg-8">
                <div class="search-padding-10">
                    <button type="button" class="btn btn-default btn-primary" ng-click="deleteSonArea()">
                        <span class="glyphicon glyphicon-remove btn_icon"></span>
                        删除
                    </button>
                    <button type="button" class="btn btn-default btn-primary" ng-click="addJian()">
                        <span class="glyphicon glyphicon-plus btn_icon"></span>
                        新建
                    </button>
                </div>
                <table>
                    <thead>
                    <tr>
                        <th style="width:80px;"><input ng-show="CheckOrdersService.checkItemRecords.length!=0" type="checkbox" id="allChoose" ng-click="allChoose('allChoose','CheckOrdersService')"/></th>
                        <th>检测项<span style="color: red">*</span></th>
                        <th>检测结果</th>
                        <th>是否过保<span style="color: red">*</span></th>
                        <th>是否售后</th>
                        <th>检测说明<span style="color: red">*</span></th>
                    </tr>
                    </thead>
                    <tbody>
                    <tr ng-repeat="item in CheckOrdersService.checkItemRecords">
                        <td style="width:80px;"><input type="checkbox" name="CheckOrdersService" ng-click="getChoose(item.id,$event,'allChoose','CheckOrdersService')" id="{{item.id}}"/></td>
                        <td><input class="form-control" ng-model="item.checkItemName"/></td>
                        <td>
                            <label class="checkbox inline">
                                <input type="checkbox" name="isAbnormal" ng-value="1" ng-model="item.isAbnormal">
                                <span>异常</span>
                            </label>
                        </td>
                        <td>
                            <select class="form-control" ng-model="item.isThroughInsurance">
                                <option value="">- -请选择- -</option>
                                <option value="0">已过保</option>
                                <option value="1">未过保</option>
                            </select>
                        </td>
                        <td>
                            <label class="checkbox inline">
                                <input type="checkbox" name="isAftermarket" ng-value="1" ng-model="item.isAftermarket">
                                <span>是</span>
                            </label>
                        </td>
                        <td>
                            <input class="form-control" ng-model="item.checkItemResult"/>
                        </td>
                    </tr>
                    </tbody>
                </table>
                <div class="sel1 tab_info">
                    <div class="row">
                        <div class="col-lg-2 col-md-2 col-sm-2 text-right">备注<span style="color: red">*</span></div>
                        <div class="col-lg-10 col-md-10 col-sm-10"><textarea class="text_min" ng-model="CheckOrdersService.remarks"></textarea></div>
                        <div class="col-lg-2 col-md-2 col-sm-2 text-right">签字人<span style="color: red">*</span></div>
                        <div class="col-lg-10 col-md-10 col-sm-10"><input class="form-control" placeholder="默认为业主" ng-model="CheckOrdersService.signatory"  data-toggle="modal" data-target="#myModalPerson1" readonly="readonly"></div>
                        <div class="col-lg-2 col-md-2 col-sm-2 text-right">
                            附件列表
                        </div>
                        <div class="col-lg-10 col-md-10 col-sm-10 text-right">
                            <button type="button" class="btn btn-default btn-primary" data-toggle="modal" data-target="#ddd" style="cursor:pointer" tabindex="-1"  ng-click="CheckOrdersServicefile()">上传 </button>
                        </div>
                    </div>
                    <div class="col-lg-10 col-md-10 col-sm-12 col-lg-offset-2 col-md-offset-2 text-left">
                    <span ng-if="annexs != null">
                        <span class="text-center" ng-repeat="item in CheckOrdersService.annexs" style="display:inline-block;margin-right:5px;">
                            <img src={{fileUrl}}/{{item.annexAddress}} style="display:block;margin-bottom:5px;width: 100px;height: 100px"><button class="btn btn-default btn-primary tab-btn" ng-click="delPic2(item);">移除</button>
                        </span>
                    </span>
                    </div>

                </div>
            </div>
            <div class="clear"></div>
            <div class="sel1 text_center">
                <button type="button" class="btn btn-default btn-primary" ng-click="colseConsultation(2)">
                    取消
                </button>
                <button type="button" class="btn btn-default btn-primary" ng-click="AddOrUpdateCheckOrdersService(CheckOrdersService.taskId)">
                    提交
                </button>
            </div>
        </div>
    </div>

    <!-- 处理单 -->
    <div id="div3" style="display:none;">
        <div id="tab">
            <a lang="no" class="this_action">处理单</a>
        </div>
        <div class="row padding">
            <div class="col-lg-2"></div>
            <div class="col-lg-8">
                <div class="row sel tab_info">
                    <div class="col-lg-2 col-md-2 col-sm-2 text-right">处理项：<span style="color: red">*</span></div>
                    <div class="col-lg-10 col-md-10 col-sm-10">
                        <input class="form-control" type="text" ng-model="processService.checkItemName" required placeholder="请输入姓名"/>
                    </div>
                    <div class="col-lg-2 col-md-2 col-sm-2 text-right">处理说明：<span style="color: red">*</span></div>
                    <div class="col-lg-10 col-md-10 col-sm-10"><textarea class="text_min" ng-model="processService.checkItemResult" required  placeholder="请输入说明"></textarea></div>
                    <div class="col-lg-2 col-md-2 col-sm-2 text-right">
                        附件列表：
                    </div>
                    <div class="col-lg-10 col-md-10 col-sm-10 text-right">
                        <button type="button" class="btn btn-default btn-primary" data-toggle="modal" data-target="#ddd" style="cursor:pointer" tabindex="-1"  ng-click="processServicefile()">上传 </button>
                    </div>
                    <div class="col-lg-10 col-md-10 col-sm-12 col-lg-offset-2 col-md-offset-2 text-left">
                    <span ng-if="annexs != null">
                        <span class="text-center" ng-repeat="item in processService.annexs" style="display:inline-block;margin-right:5px;">
                            <img src={{fileUrl}}/{{item.annexAddress}} style="display:block;margin-bottom:5px;width: 100px;height: 100px"><button class="btn btn-default btn-primary tab-btn" ng-click="delPic23(item);">移除</button>
                        </span>
                    </span>
                    </div>
                </div>
            </div>
            <div class="clear"></div>
        </div>
        <div class="row search-padding-10 text_center">
            <button type="button" class="btn btn-default btn-primary" ng-click="colseConsultation(23)">
                取消
            </button>
            <button type="button" class="btn btn-default btn-primary" ng-click="addOrUpdateProcess()">
                提交
            </button>
        </div>
    </div>

    <!-- 验收单 -->
    <div id="div4" style="display:none;">
        <div id="tab">
            <a lang="no" class="this_action">验收单</a>
        </div>
        <div class="row padding">
            <div class="col-lg-2"></div>
            <div class="col-lg-8 tab_info">
                <div class="row sel1">
                    <div class="col-lg-2 col-md-2 col-sm-2 text-right">验收结果<span style="color: red">*</span></div>
                    <div class="col-lg-2 col-md-2 col-sm-2">
                        <label class="radio inline">
                            <!-- ng-model="AcceptanceOrders.acceptanceItemResult" -->
                            <input type="radio" checked="checked" name="acceptanceItemResult" ng-model="AcceptanceOrders.acceptanceItemResult" ng-value="0">
                            <span>合格</span>
                        </label>
                    </div>
                    <div class="col-lg-3 col-md-2 col-sm-2">
                        <label class="radio inline">
                            <input type="radio" name="acceptanceItemResult" ng-model="AcceptanceOrders.acceptanceItemResult" ng-value="1">
                            <span>不合格</span>
                        </label>
                    </div>
