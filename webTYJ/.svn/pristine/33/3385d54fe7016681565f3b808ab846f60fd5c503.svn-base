                                    <ul class="pagination">
                                        <li ng-class="{true:'disabled'}[ElectricityPaginator.page.currentPage==1]">
                                            <a href="javascript:void(0)" ng-click="ElectricityPaginator.go(1)">首页</a>
                                        </li>
                                        <li ng-class="{true:'disabled'}[ElectricityPaginator.page.currentPage==1]">
                                            <a href="javascript:void(0)" ng-show="ElectricityPaginator.hasPrevious()" ng-click="ElectricityPaginator.previous()">上一页</a>
                                        </li>
                                        <li ng-class="{true:'active'}[ElectricityPaginator.page.currentPage==i]" ng-repeat="i in ElectricityPaginator.pages">
                                            <a href="javascript:void(0)"  ng-click="ElectricityPaginator.go(i)">{{i}}</a>
                                        </li>
                                        <li ng-class="{true:'disabled'}[ElectricityPaginator.page.currentPage==ElectricityPaginator.page.totalPage]">
                                            <a href="javascript:void(0)" ng-show="ElectricityPaginator.hasNext()" ng-click="ElectricityPaginator.next()">下一页</a>
                                        </li>
                                        <li ng-class="{true:'disabled'}[ElectricityPaginator.page.currentPage==ElectricityPaginator.page.totalPage]">
                                            <a href="javascript:void(0)" ng-click="ElectricityPaginator.go(ElectricityPaginator.page.totalPage)">尾页</a>
                                        </li>
                                    </ul>
                                </div>
                            </div>
                            <div class="col-lg-12 col-md-12 col-sm-12 text_center">
                                <div ng-if="aa.meterReadingOrders.annexs[0]!=null">
                                    <div class="col-md-2" ng-repeat="annex in aa.meterReadingOrders.annexs">
                                        <img src="{{fileUrl}}/{{annex.annexAddress}}" width="100%">
                                    </div>
                                </div>
                                <div class="col-lg-12 col-md-12 col-sm-12 ">
                         <span ng-repeat="annex in aa.meterReadingOrders.annexs"  >
                            <img src="{{fileUrl}}/{{annex.annexAddress}}" style="height: 100px;width: 100px">
                        </span>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
