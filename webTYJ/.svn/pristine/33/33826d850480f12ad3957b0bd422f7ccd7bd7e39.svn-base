    <div class="row padding">
        <div class="col-lg-12">
            <div class="col-lg-4 col-md-6 col-sm-6"style="cursor: pointer">
                <div id="pannel">
                    <div class="title">
                        <div class="col-xs-6 col-lg-6 col-md-6 col-sm-6">
                            <h3 class="text-left">添加装修服务请求</h3>
                        </div>
                    </div>
                    <div class="pannel_content height"  ng-click="add1()" >
                        <div class="col-md-12 text-center">
                            <a ui-sref="index.propertyService.staffHome.decorate" class="changeBorder" style="cursor: pointer">
                                <img src="images/icon/mn/add.png" style="width: 100px;height: 100px" >
                            </a>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-lg-4 col-md-6 col-sm-6"ng-repeat="ite in ServiceRequest" style="cursor: pointer">
                <div id="pannel">
                    <div class="downList">
                       <!-- <div class="col-xs-12 col-lg-12 col-md-12 col-sm-12 text-right">
                                <span class="glyphicon glyphicon-chevron-down" id="dropdownMenu1" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true"></span>
                            <ul class="dropdown-menu" aria-labelledby="dropdownMenu1">
                                <li><a>失效</a></li>
                                <li><a>完成</a></li>
                            </ul>
                        </div>-->
                    </div>
                    <div class="title">
                        <div class="col-xs-6 col-lg-6 col-md-6 col-sm-6">
                            <h3 class="text-left">装修服务请求</h3>
                        </div>
                    </div>
                    <div class="pannel_content height"  ng-click="add(ite)" >
                        <div class="col-md-5">
                            <a href="#" data-toggle="modal" data-target="#modifyPete" ng-click="getStallId(stall)">
                                <img src="images/project/bud/b_01.png" width="100" height="90" >
                            </a>
                        </div>
                        <div class="col-md-7">
                            <div class="row">客户姓名：{{user.name}}</div>
                            <div class="row">申请时间：{{ite.requestTime|date : "yyyy-MM-dd HH:ss"}}</div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row padding">
        <div class="col-lg-12">
            <div ui-view></div>
        </div>
    </div>
</div>
