<!DOCTYPE html>
<html>
<head lang="en">
    <meta charset="UTF-8">
    <title>电表抄表服务请求管理</title>
    <link rel="stylesheet" href="css/ztree/demo.css" type="text/css">
    <link rel="stylesheet" href="css/ztree/zTreeStyle.css" type="text/css">
    <script type="text/javascript" src="bower_components/ztree/jquery.ztree.core-3.5.js"></script>
    <script type="text/javascript" src="bower_components/ztree/jquery.ztree.excheck-3.5.js"></script>
    <script type="text/javascript" src="bower_components/ztree/jquery.ztree.exedit-3.5.js"></script>
</head>
<body>
<div id="content-first">
    <!--tab页切换开始-->
    <div id="tab">
        <a ng-class="{'this_action ':btnIndex==1}" href="javascript:void(0)" ng-click="service()" >服务请求</a>
        <a ng-class="{'this_action ':btnIndex==2}" href="javascript:void(0)" ng-click="datils()" ng-show="datils1">服务请求详情</a>
        <a ng-class="{'this_action ':btnIndex==3}" href="javascript:void(0)" ng-click="tasks()" ng-show="tasks1">任务详情</a>
        <a ng-class="{'this_action ':btnIndex==4}" href="javascript:void(0)" ng-click="addService()" ng-show="addService1">新建服务请求</a>
        <a ng-class="{'this_action ':btnIndex==5}" href="javascript:void(0)" lang="no"  ng-click="addDatils()"  ng-show="addDatils1">新建任务</a>
    </div>
    <!--tab页切换结束-->
    <!--服务请求列表开始-->
    <div class="row padding" ng-show="show1">
        <div class="row search-padding-10">
            <div class="col-lg-1 col-md-1 col-sm-1 text-right">
                搜索条件
            </div>
            <div class="col-lg-2 col-md-2 col-sm-2">
                <input class="form-control" ng-model="ServiceRequest.remarks"/>
            </div>
            <div class="col-lg-9 col-md-9 col-sm-9 text-right">
                <button type="button" class="btn btn-default btn-primary tab-btn" ng-click="searchPaginator._load();">
                    <span class="glyphicon glyphicon-search btn_icon"></span>
                    搜索
                </button>
            </div>
        </div>
        <div class="row search-padding-10">
            <div class="col-lg-10 col-md-10 col-sm-10">
                <div class="col-lg-1 col-md-1 col-sm-1">
                    <button type="button" class="btn btn-default btn-primary tab-btn" ng-click="addService()">
                        <span class="glyphicon glyphicon-plus btn_icon"></span>
                        新建
                    </button>
                </div>
                <div class="col-lg-11 col-md-11 col-sm-11" ng-show="requestShowStatus==0">
                    <button type="button" class="btn btn-default btn-primary tab-btn" ng-click="selectedAll('request')">
                        <span class="glyphicon glyphicon-check btn_icon"></span>
                        全选
                    </button>
                    <button type="button" class="btn btn-default btn-primary tab-btn" data-target="#serviceClosedAll"
                            data-toggle="modal">
                        <span class="glyphicon glyphicon-ban-circle btn_icon"></span>
                        失效
                    </button>
                    <button type="button" class="btn btn-default btn-primary tab-btn" data-target="#serviceFinishAll"
                            data-toggle="modal">
                        <span class="glyphicon glyphicon-ok btn_icon"></span>
                        完成
                    </button>
                </div>
            </div>
            <div class="col-lg-2 col-md-2 col-sm-2 text-right">
                <button type="button" class="btn btn-default btn-primary tab-btn"
                        ng-click="viewChange('requestShowStatus','1')">
                    <span class="glyphicon glyphicon-th-large btn_icon"></span>
                    网格
                </button>
                <button type="button" class="btn btn-default btn-primary tab-btn"
                        ng-click="viewChange('requestShowStatus','0')">
                    <span class="glyphicon glyphicon-th-list btn_icon"></span>
                    列表
                </button>
            </div>
        </div>

        <div class="col-lg-12" ng-show="requestShowStatus==1">
            <div class="col-lg-4 col-md-6 col-sm-6 ng-scope" ng-repeat="item in searchPaginator.object.serviceRequests"
                 ng-show="item.serviceRequestId != null">
                <div id="pannel">
                    <div class="downList">
                        <div class="col-xs-12 col-lg-12 col-md-12 col-sm-12 text-right">
                    <span class="glyphicon glyphicon-chevron-down" id="dropdownMenu1" data-toggle="dropdown"
                          aria-haspopup="true" aria-expanded="true">
                     </span>
                            <ul class="dropdown-menu" aria-labelledby="dropdownMenu1">
                                <li><a class="program_panel_info_navigation_top" data-toggle="modal"
                                       data-target="#serviceFinish" style="cursor:pointer"
                                       ng-click="servicechange(item)">完成</a></li>
                                <li><a data-toggle="modal" data-target="#serviceClosed" style="cursor:pointer"
                                       ng-click="servicechange(item)">失效</a></li>
                                <li><a style="cursor:pointer" ng-click="selectTasks(item)">查看</a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="title">
                        <div class="col-xs-6 col-lg-6 col-md-6 col-sm-6">
                            <h3>查抄电表</h3>
                        </div>
                    </div>
                    <div class="pannel_content" style="cursor: pointer" ng-click="selectTasks(item)">
                        <div class="row search-padding-10">
                            <div class="col-lg-6 col-md-6 col-sm-6">客户姓名：</div>
                            <div class="col-lg-6 col-md-6 col-sm-6 text-right">{{item.personCustNew.name}}</div>
                        </div>
                        <div class="row search-padding-10">
                            <div class="col-lg-6 col-md-6 col-sm-6">请求类型：</div>
                            <div class="col-lg-6 col-md-6 col-sm-6 text-right">查抄电表</div>
                        </div>
                        <div class="row search-padding-10">
                            <div class="col-lg-6 col-md-6 col-sm-6">状态：</div>
                            <div class="col-lg-6 col-md-6 col-sm-6 text-right" ng-switch on="item.serviceRequestState">
                                <span ng-switch-when="0">关闭</span>
                                <span ng-switch-when="1">启用</span>
                                <span ng-switch-when="2">处理中</span>
                                <span ng-switch-when="3">完成</span>
                            </div>
                        </div>

                        <div class="row search-padding-10">
                            <div class="col-lg-6 col-md-6 col-sm-6">创建时间：</div>
                            <div class="col-lg-6 col-md-6 col-sm-6 text-right">{{item.requestTime|date:"yyyy-MM-dd"}}
                            </div>
                        </div>
                        <div class="row search-padding-10">
                            <div class="col-lg-6 col-md-6 col-sm-6">备注：</div>
                            <div class="col-lg-6 col-md-6 col-sm-6 text-right">{{item.remarks}}</div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row search-padding-10" ng-show="requestShowStatus==0">
            <table>
                <thead>
                <tr>
                    <th>客户姓名</th>
                    <th>请求类型</th>
                    <th>状态</td>
                    <th>创建时间</th>
                    <th>备注</th>
                </tr>
                </thead>
                <tbody id="request">
                <tr ng-repeat="item in searchPaginator.object.serviceRequests" ng-show="item.serviceRequestId != null"
                    ng-dblclick="selectTasks(item)"
                    ng-click="selected(item.serviceRequestId,item)"
                    id="{{item.serviceRequestId}}">
                    <td>{{item.personCustNew.name}}</td>
                    <td><!--{{item.serviceRequestType}}-->查抄电表</td>
                    <td ng-switch on="item.serviceRequestState">
                        <span ng-switch-when="0">关闭</span>
                        <span ng-switch-when="1">启用</span>
                        <span ng-switch-when="2">处理中</span>
                        <span ng-switch-when="3">完成</span>
                    </td>
                    <td>{{item.requestTime|date:"yyyy-MM-dd"}}</td>
                    <td>{{item.remarks}}</td>
                </tr>
                </tbody>
            </table>
        </div>

        <div class="bill_paging">
            <span>共{{searchPaginator.object.page.totalResult}}条</span>
            <ul class="pagination">
                <li ng-class="{true:'disabled'}[searchPaginator.page.currentPage==1]">
                    <a href="javascript:void(0)" ng-click="searchPaginator.go(1)">首页</a>
                </li>
                <li ng-class="{true:'disabled'}[searchPaginator.page.currentPage==1]">
                    <a href="javascript:void(0)" ng-show="searchPaginator.hasPrevious()"
                       ng-click="searchPaginator.previous()">上一页</a>
                </li>
                <li ng-class="{true:'active'}[searchPaginator.page.currentPage==i]"
                    ng-repeat="i in searchPaginator.pages">
                    <a href="javascript:void(0)" ng-click="searchPaginator.go(i)">{{i}}</a>
                </li>
                <li ng-class="{true:'disabled'}[searchPaginator.page.currentPage==searchPaginator.page.totalPage]">
                    <a href="javascript:void(0)" ng-show="searchPaginator.hasNext()"
                       ng-click="searchPaginator.next()">下一页</a>
                </li>
                <li ng-class="{true:'disabled'}[searchPaginator.page.currentPage==searchPaginator.page.totalPage]">
                    <a href="javascript:void(0)"
                       ng-click="searchPaginator.go(searchPaginator.page.totalPage)">尾页</a>
                </li>
            </ul>
        </div>
    </div>
    <!--服务请求列表结束-->
    <!--任务列表开始-->
    <div ng-show="show2">
        <div class="sel">
            <div class="row">
                <div class="col-lg-8 col-md-6 col-sm-6">
                    <button type="button" class="btn btn-default btn-primary tab-btn" ng-click="backToRequest()">
                        <span class="glyphicon glyphicon-arrow-left btn_icon"></span>
                        返回
                    </button>
                    <button type="button" class="btn btn-default btn-primary tab-btn" ng-click="allTasksChecked()">
                        <span class="glyphicon glyphicon-check btn_icon"></span>
                        全选
                    </button>
                    <button type="button" class="btn btn-default btn-primary tab-btn" data-target="#concentrateds" data-toggle="modal" >
                        <span class="glyphicon glyphicon-screenshot btn_icon"></span>
                        集中处理项
                    </button>
                    <button type="button" class="btn btn-default btn-primary tab-btn"   data-target="#turns" data-toggle="modal" >
                        <span class="glyphicon glyphicon-new-window btn_icon"></span>
                        转专项
                    </button>
                    <button type="button" class="btn btn-default btn-primary tab-btn"  data-target="#frees" data-toggle="modal"  >
                        <span class="glyphicon glyphicon-log-out btn_icon"></span>
                        转出
                    </button>
                    <button type="button" class="btn btn-default btn-primary tab-btn"   data-target="#finishs" data-toggle="modal" >
                        <span class="glyphicon glyphicon-ok btn_icon"></span>
                        完成
                    </button>
                    <button type="button" class="btn btn-default btn-primary tab-btn" data-target="#close" data-toggle="modal" >
                        <span class="glyphicon glyphicon-ban-circle btn_icon"></span>
                        失效
                    </button>
                    <button type="button" class="btn btn-default btn-primary tab-btn" ng-click="verifyAccounting()">
                        <span class="glyphicon glyphicon-th-large btn_icon"></span>
                        合并
                    </button>
                    <button type="button" class="btn btn-default btn-primary tab-btn" ng-click="addTasks(Service)">
                        <span class="glyphicon glyphicon-plus btn_icon"></span>
                        新建
                    </button>
                </div>
                <div class="col-lg-1 col-md-2 col-sm-2 text-right">
                    搜索条件
                </div>
                <div class="col-lg-2 col-md-4 col-sm-4">
                    <input class="form-control" ng-model="Tasks.taskDescription"/>
                </div>
                <div class="col-lg-1 col-md-2 col-sm-2">
                    <button type="button" class="btn btn-default btn-primary tab-btn" ng-click="searchPaginator1._load();">
                        <span class="glyphicon glyphicon-search btn_icon"></span>
                        搜索
                    </button>
                </div>
            </div>
        </div>
        <div class="sel">
            <div class="row">
                <div class="col-lg-1 col-md-2 col-sm-2">
                    <img src="images/project/user/user_3.png" width="80" height="80">
                </div>
                <div class="col-lg-5 col-md-4 col-sm-4">
                    <div class="row search-padding-10">
                        <div class="col-lg-6 col-md-6 col-sm-6">客户姓名：{{Service.personCustNew.name}}</div>
                        <div class="col-lg-6 col-md-6 col-sm-6">创建时间：{{Service.requestTime | date:"yyyy-MM-dd"}}</div>
                        <div class="col-lg-6 col-md-6 col-sm-6">请求类型：查抄电表</div>
                        <div class="col-lg-6 col-md-6 col-sm-6">创建人：{{Service.personCustNew.name}}</div>
                    </div>
                </div>
                <div class="col-lg-6 col-md-6 col-sm-6" style="word-wrap: break-word">
                    <div class="row search-padding-10">
                        <div class="col-lg-12 col-md-12 col-sm-12">
                            描述：{{ Service.remarks}}
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row padding">
            <div class="row search-padding-10">
                <div class="col-lg-10 col-md-10 col-sm-10">
                    <div ng-show="workShowStatus==0">
                        <button type="button" class="btn btn-default btn-primary tab-btn" ng-click="selectedAll('task')">
                            <span class="glyphicon glyphicon-check btn_icon"></span>
                            全选
                        </button>
                        <button type="button" class="btn btn-default btn-primary tab-btn" ng-click="allTasksChecked()">
                            <span class="glyphicon glyphicon-hand-right btn_icon"></span>
                            接受
                        </button>
                        <button type="button" class="btn btn-default btn-primary tab-btn" ng-click="allTasksChecked()">
                            <span class="glyphicon glyphicon-log-out btn_icon"></span>
                            转出
                        </button>
                        <button type="button" class="btn btn-default btn-primary tab-btn" ng-click="allTasksChecked()">
                            <span class="glyphicon glyphicon-time btn_icon"></span>
                            延时
                        </button>
                        <button type="button" class="btn btn-default btn-primary tab-btn" ng-click="allTasksChecked()">
                            <span class="glyphicon glyphicon-step-forward btn_icon"></span>
                            催促
                        </button>
                        <button type="button" class="btn btn-default btn-primary tab-btn" ng-click="allTasksChecked()">
                            <span class="glyphicon glyphicon-ok btn_icon"></span>
                            完成
                        </button>
                        <button type="button" class="btn btn-default btn-primary tab-btn" ng-click="allTasksChecked()">
                            <span class="glyphicon glyphicon-hand-left btn_icon"></span>
                            指派
                        </button>
                        <button type="button" class="btn btn-default btn-primary tab-btn" ng-click="allTasksChecked()">
                            <span class="glyphicon glyphicon-remove btn_icon"></span>
                            关闭
                        </button>
                        <button type="button" class="btn btn-default btn-primary tab-btn" ng-click="allTasksChecked()">
                            <span class="glyphicon glyphicon-share btn_icon"></span>
                            转专项
                        </button>
                        <button type="button" class="btn btn-default btn-primary tab-btn" ng-click="allTasksChecked()">
                            <span class="glyphicon glyphicon-screenshot btn_icon"></span>
                            集中处理
                        </button>
                    </div>
                </div>
                <div class="col-lg-2 col-md-2 col-sm-2 text-right">
                    <button type="button" class="btn btn-default btn-primary tab-btn" ng-click="viewChange('workShowStatus','1')">
                        <span class="glyphicon glyphicon-th-large btn_icon"></span>
                        网格
                    </button>
                    <button type="button" class="btn btn-default btn-primary tab-btn"  ng-click="viewChange('workShowStatus','0')" >
                        <span class="glyphicon glyphicon-th-list btn_icon"></span>
                        列表
                    </button>
                </div>
            </div>
            <div class="col-lg-12" ng-show="workShowStatus==1">
                <div class="col-lg-4 col-md-6 col-sm-6 ng-scope" ng-repeat="item1 in searchPaginator1.object.tasks" ng-show="item1.taskId != null">
                    <div id="pannel">
                        <div class="downList">
                            <div class="col-xs-12 col-lg-12 col-md-12 col-sm-12 text-right" >
                                <span class="glyphicon glyphicon-chevron-down"id="dropdownMenu1" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true"></span>
                                <ul class="dropdown-menu" aria-labelledby="dropdownMenu1">
                                    <li> <a class="program_panel_info_navigation_top" style="cursor:pointer"  ng-click="updateAccept(item1)">接受</a></li>
                                    <li> <a data-toggle="modal" data-target="#free" style="cursor:pointer"  ng-click="change(item1)">转出</a></li>
                                    <li> <a data-toggle="modal" data-target="#delayed" ng-click="change(item1)">延时</a></li>
                                    <li> <a data-toggle="modal" data-target="#press" ng-click="change(item1)">催促</a></li>
                                    <li> <a data-toggle="modal" data-target="#finish" style="cursor:pointer" ng-click="change(item1)">完成</a></li>
                                    <li> <a data-toggle="modal" data-target="#assign" style="cursor:pointer"  ng-click="change(item1)">指派</a></li>
                                    <li> <a data-toggle="modal" data-target="#closed" style="cursor:pointer"ng-click="change(item1)">关闭</a></li>
                                    <li> <a data-toggle="modal" data-target="#turn" style="cursor:pointer"ng-click="change(item1)">转专项</a></li>
                                    <li> <a class="program_panel_info_navigation_bottom" data-toggle="modal" data-target="#concentrated" style="cursor:pointer" ng-click="change(item1)">集中处理</a></li>
                                </ul>
                            </div>
                        </div>
                        <div class="title">
                            <div class="col-xs-6 col-lg-6 col-md-6 col-sm-6"  ng-switch on="item1.taskType">
                                <h3 ng-switch-when="0" class="text-left">园林</h3>
                                <h3 ng-switch-when="1">维修</h3>
                                <h3 ng-switch-when="2">护管</h3>
                                <h3 ng-switch-when="9">赔偿给业主</h3>
                                <h3 ng-switch-when="10">向业主索赔</h3>
                                <h3 ng-switch-when="11">清洁</h3>
                                <h3 ng-switch-when="12">回访</h3>
                                <h3 ng-switch-when="13">投诉</h3>
                                <h3 ng-switch-when="14">验房</h3>
                                <h3 ng-switch-when="15">咨询</h3>
                                <h3 ng-switch-when="16">抄水表</h3>
                                <h3 ng-switch-when="17">抄电表</h3>
                                <h3 ng-switch-when="18">装修核查任务</h3>
                                <h3 ng-switch-when="4">装修巡检任务</h3>
                                <h3 ng-switch-when="5">装修验收任务</h3>
                            </div>
                        </div>
                        <div class="pannel_content" ng-click="taskDetailsIds(item1.taskId)"  >
                            <div class="row search-padding-10">
                                <div class="col-lg-6 col-md-6 col-sm-6">任务类型：</div>
                                <div class="col-lg-6 col-md-6 col-sm-6 text-right">
                                    <span ng-switch on="item1.taskType">
                                    <span ng-switch-when="0">园林</span>
                                    <span ng-switch-when="1">维修</span>
                                    <span ng-switch-when="2">护管</span>
                                  <span ng-switch-when="18">装修核查任务</span>
                                    <span ng-switch-when="4">装修巡检任务</span>
                                    <span ng-switch-when="5">装修验收任务</span>
                                    <!--<span ng-switch-when="6">施工巡检</span>-->
                                    <!--<span ng-switch-when="7">施工核查</span>-->
                                    <!--<span ng-switch-when="8">施工验收</span>-->
                                    <span ng-switch-when="9">赔偿给业主</span>
                                    <span ng-switch-when="10">向业主索赔</span>
                                    <span ng-switch-when="11">清洁</span>
                                    <span ng-switch-when="12">回访</span>
                                    <span ng-switch-when="13">投诉</span>
                                    <span ng-switch-when="14">验房</span>
                                    <span ng-switch-when="15">咨询</span>
                                    <span ng-switch-when="16">抄水表</span>
                                    <span ng-switch-when="17">抄电表</span>
                                        </span>
                                </div>
                            </div>
                            <div class="row search-padding-10">
                                <div class="col-lg-6 col-md-6 col-sm-6">负责人：</div>
                                <div class="col-lg-6 col-md-6 col-sm-6 text-right">{{item1.staff.staffName}}</div>
                            </div>
                            <div class="row search-padding-10">
                                <div class="col-lg-6 col-md-6 col-sm-6">状态：</div>
                                <div class="col-lg-6 col-md-6 col-sm-6 text-right">  <span ng-switch on="item1.taskState">
                                        <span ng-switch-when="0">失效</span>
                                        <span ng-switch-when="1">重启</span>
                                        <span ng-switch-when="2">已完成</span>
                                        <span ng-switch-when="3">已接受</span>
                                        <span ng-switch-when="4">未接受</span>
                                        <span ng-switch-when="5">转出</span>
                                        <span ng-switch-when="6">提交完成</span>
                                        <span ng-switch-when="7">处理中</span>
                                        <span ng-switch-when="8">合并</span>
                                        <span ng-switch-when="9">转专项</span>
                                        <span ng-switch-when="10">转集中处理</span>
                                        <span ng-switch-when="11">指派</span>
                                        </span></div>
                            </div>
                            <div class="row search-padding-10">
                                <div class="col-lg-6 col-md-6 col-sm-6">创建时间：</div>
                                <div class="col-lg-6 col-md-6 col-sm-6 text-right">{{item1.createTime}}</div>
                            </div>
                            <div class="row search-padding-10">
                                <div class="col-lg-6 col-md-6 col-sm-6">备注：</div>
                                <div class="col-lg-6 col-md-6 col-sm-6 text-right">{{item1.taskDescription}}</div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row search-padding-10" ng-show="workShowStatus==0">
                <table>
                    <thead>
                        <tr>
                            <th>任务类型</th>
                            <th>负责人</th>
                            <th>状态</th>
                            <th>创建时间</th>
                            <th>备注</th>
                        </tr>
                    </thead>
                    <tbody id="task">
                        <tr ng-repeat="item1 in searchPaginator1.object.tasks" ng-show="item1.taskId != null"
                                ng-dblclick="taskDetailsIds(item1.taskId)"
                                ng-click="selected(item1.taskId)"
                                id="{{item1.taskId}}">
                            <td>抄电表</td>
                            <td>{{item1.staff.staffName}}</td>
                            <td>
                                <span ng-switch on="item1.taskState">
                                    <span ng-switch-when="0">失效</span>
                                    <span ng-switch-when="1">重启</span>
                                    <span ng-switch-when="2">已完成</span>
                                    <span ng-switch-when="3">已接受</span>
                                    <span ng-switch-when="4">未接受</span>
                                    <span ng-switch-when="5">转出</span>
                                    <span ng-switch-when="6">提交完成</span>
                                    <span ng-switch-when="7">处理中</span>
                                    <span ng-switch-when="8">合并</span>
                                    <span ng-switch-when="9">转专项</span>
                                    <span ng-switch-when="10">转集中处理</span>
