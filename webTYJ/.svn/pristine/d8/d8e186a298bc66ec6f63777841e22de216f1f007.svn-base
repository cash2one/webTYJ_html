        //去重人员
        $scope.notrepeat=function(entrances){
            for ( var i = 0; i < entrances.length; i++) {
                for(var j = entrances.length-1 ; j > i; j--){
                    if (entrances[i].custId == entrances[j].custId) {
                        entrances[j]={};
                    }
                }
                //     console.log(entrances);
            }
            return entrances;

