.clearfix:after{content:'\0020';display:block;height:0;clear:both}#logo{padding-top:26px;padding-bottom:12px}
.b_inputAttention .attention,.cate-path .up,.cate-tip i,.cateBtn,.cateBtn button,.catePublish,.category-search .search-hint i,.cc-cbox-filter input,.cc-hasChild-item,.cc-nav,.cc-tree-gname,.search-result
.trigger-exit i{background:url(http://img02.taobaocdn.com/tps/i2/T13PGlXfdfXXXXXXXX-261-188.png) -999em -999em no-repeat}
.cate-container{width:950px;position:relative;border:1px solid #d5e4fa;background-color:#F5F9FF}
.cate-main{float:left;padding-top:10px;border-right:1px solid #d5e4fa;width:725px}
.cc-loading-text{font-size:12px;display:block;float:left;margin-left:5px;padding-top:0;color:#f60}
.loading .cc-loading{display:block}
.cate-path{color:#444;padding-top:9px;position:relative;line-height:22px;height:54px}
.cate-path dl{background-color:#FFFAF2;border:1px solid #FEDBAB;padding:4px 27px}
.cate-path dt{font-weight:900}
.cate-path dd,.cate-path dt,.cate-path li{float:left}
.cate-desc{clear:both;display:none;color:#F60;width:780px;height:22px;overflow:hidden;white-space:nowrap}
.cate-path .up{height:7px;left:43px;top:3px;width:13px;position:absolute;background-position:-223px -60px}
.cate-container{height:300px;overflow:hidden}

#cate-cascading{padding:0 24px;height:290px;position:relative;z-index:11}
.cc-nav{visibility:hidden;position:absolute;top:50%;margin-top:-41px;height:80px;width:24px;display:inline-block;line-height:99em;overflow:hidden}
#cate-cascading .cc-prev{left:0;background-position:0 -60px}
#cate-cascading .cc-next{right:0;background-position:-24px -60px}
.cc-listwrap{border-left:1px solid #d5e4fa;border-right:1px solid #d5e4fa;border-top:1px solid #d5e4fa;position:relative;overflow:hidden}
.cc-list{width:2000em;position:absolute;left:0;top:0}
.cc-cbox,.cc-list,.cc-list-item,.cc-listwrap,.cc-tree{height:100%}.cc-cbox,.cc-tree{overflow-x:hidden;overflow-y:auto;position:relative;left:0;top:0}
.cc-list-item{float:left;width:223px;border-right:2px solid #d5e4fa;background:#fff}
.cc-listbox{padding:3px}.cc-cbox-cont,.cc-tree-cont{padding:0 3px;margin-top:26px}
.cc-cbox-group{position:relative}
.cc-cbox-gname{float:left;background:#AAA;color:#FFF;display:inline-block;font-style:normal;height:13px;line-height:13px;text-align:center;text-transform:uppercase;width:13px;position:absolute;margin-left:16px;margin-top:5px;overflow:hidden}
.cc-tree-group{padding:3px 0;border-bottom:1px dashed #ccc;zoom:1}
.cc-tree .cc-tree-last{border:none}
.cc-tree-gname{color:#36c;background-position:right -107px;display:inline-block;margin:2px 0;cursor:default}
.cc-tree-expanded .cc-tree-gname{background-position:right -123px}
.cc-cbox-item,.cc-listbox-item,.cc-tree-item{background-color:#FFF;border:1px solid #FFF;height:20px;line-height:20px;overflow:hidden;cursor:pointer;padding-left:41px}
.cc-cbox-item{padding-right:16px}.cc-listbox-item,.cc-tree-gname,.cc-tree-item{padding:0 16px 0 14px}
.cc-tree-gcont{display:none}.cc-tree-expanded .cc-tree-gcont{display:block}
li.cc-selected{border:1px solid #9cd7fc;background-color:#dff1fb}
li.cc-focused{border:1px dotted #82bce0}div.cc-focused{outline:#9cd7fc dotted 1px}
li.cc-hasChild-item{background-position:right -137px}
.cc-cbox-filter{padding:2px 3px;background:#f5f9ff;position:absolute;top:0;z-index:10}
.cc-cbox-filter label{color:#AAA;padding-left:29px;position:absolute;top:3px}
.cc-cbox-filter input{background-color:#fff;background-position:-239px -89px;border:1px solid #D9E5F6;height:20px;padding-left:23px;width:176px}
