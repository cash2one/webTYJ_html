<!--auhor:王志刚-->
<!--页面名称：物业服务装修主页面-->
<!DOCTYPE html>
<html>
<head lang="en">
    <meta charset="UTF-8">
    <title>人员信息里的装修</title>
    <link rel="stylesheet" href="css/propertyService.css">
    <link rel="stylesheet" href="bower_components/skins/zyupload-1.0.0.min.css " type="text/css">
    <style type="text/css">
        #pm img{width:80px;height:80px;}
        .doted{ height:20px; text-align:center; width:80px; margin:0 auto; }
        .doted .dot_unChecked{ display:block; width:25px; height:25px; background:#e5e5e7; border:1px solid #a4a4a4; -moz-border-radius:50%; -webkit-border-radius:50%; border-radius:50%; float:left; margin-left:50px; margin-top:10px;}
        .doted .dot_Check{ display:block; width:25px; height:25px; background:#02a0e7; -moz-border-radius:50%; -webkit-border-radius:50%; border-radius:50%; float:left; margin-left:50px; margin-top:10px;}
        .doted .dot_Check{ display:block; width:25px; height:25px; background:#02a0e7; -moz-border-radius:50%; -webkit-border-radius:50%; border-radius:50%; float:left; margin-left:50px; margin-top:10px;}
        .doted .dot_unChecked:hover{ display:block; width:25px; height:25px; background:#02a0e7; -moz-border-radius:50%; -webkit-border-radius:50%; border-radius:50%; float:left; margin-left:50px; margin-top:10px;}
        .connectLines hr{width:76px;text-align: center;border: 1px dotted rgb(180,180,180);position: absolute;margin: 0 0 0 76px;}
    </style>
</head>
<body>
<div class="padding_info">
<div id="content-first" >
    <div id="tab">
        <a lang="no" class="this_action">业主申请装修</a>
    </div>
    <div class="sel">
        <div class="row">
            <div class="col-md-1"></div>
            <div class="col-md-1">
                <span style="margin-left:-50px">选择房屋</span>
            </div>
            <div class="col-md-1">
                <span style="margin-left:-50px">申请资料</span>
            </div>
            <div class="col-md-1">
                <span style="margin-left:-50px">现场核查</span>
            </div>
            <div class="col-md-1">
                <span style="margin-left:-50px">缴费</span>
            </div>
            <div class="col-md-1">
                <span style="margin-left:-50px">发放许可</span>
            </div>
            <div class="col-md-1">
                <span style="margin-left:-50px">费用结算</span>
            </div>
            <div class="col-md-5">&nbsp;</div>
            <div class="col-md-1">
                <span>
                    <div class="doted" style="cursor:pointer">
                        <a><span class="dot_unChecked" ng-class="{'dot_Check': btnIndex == 1}" ng-click="chooseHouse(1)"></span></a>
                    </div>
                    <div class="connectLines">
                        <hr>
                    </div>
                </span>
            </div>
            <div class="col-md-1">
                <span>
                    <div class="doted" style="cursor:pointer">
                        <a><span class="dot_unChecked" ng-class="{'dot_Check': btnIndex == 2}" ng-click="applicationData(2)"></span></a>
                    </div>
                    <div class="connectLines">
                        <hr>
                    </div>
                </span>
            </div>
            <div class="col-md-1">
                <span>
                    <div class="doted" style="cursor:pointer">
                        <a><span class="dot_unChecked" ng-class="{'dot_Check': btnIndex == 3}" ng-click="verification(3)"></span></a>
                    </div>
                    <div class="connectLines">
                        <hr>
                    </div>
                </span>
            </div>
            <div class="col-md-1">
                <span>
                    <div class="doted" style="cursor:pointer">
                        <a><span class="dot_unChecked" ng-class="{'dot_Check': btnIndex == 4}" ng-click="payDeposit(4)"></span></a>
                    </div>
                    <div class="connectLines">
                        <hr>
                    </div>
                </span>
            </div>
            <div class="col-md-1">
                <span>
                    <div class="doted">
                        <a><span class="dot_unChecked" ng-class="{'dot_Check': btnIndex == 5}" ng-click="Alicence(5)"></span></a>
                    </div>
                    <div class="connectLines">
                        <hr>
                    </div>
                </span>
            </div>
            <div class="col-md-1">
                <span>
                    <div class="doted">
                        <a><span class="dot_unChecked" ng-class="{'dot_Check': btnIndex == 6}" ng-click="costClearing(6)"></span></a>
                    </div>
                </span>
            </div>
            <div class="col-md-6"></div>
        </div>
    </div>

<div ng-show="show1">
    <div class="row padding">
        <div class="col-lg-12">
            <!--业主信息-->

            <div class="info_lista" ng-repeat="item in HouseNew" ng-click="check(item)" style="cursor: pointer">
                <div class="media-left">
                    <a href="#">
                        <img src="images/propertyService/choiceHousing/u81.jpg" width="50px;" height="50px" style="margin-top: 5px;">
                    </a>
                </div>
                <div class="media-body" >
                    <div class="row">
                        <div class="col-md-1 text-right">房屋地址：</div>
                        <div class="col-md-3 ">{{item.fullName}}</div>
                        <div class="col-md-2 text-right">业主姓名：</div>
                        <div class="col-md-2 ">{{user.name}}</div>
                        <div class="col-md-2 text-right">房屋编码：</div>
                        <div class="col-md-2 ">{{item.houseNum}}</div>
                    </div>
                    <div class="row">
                        <div class="col-md-1 text-right">入伙状态：</div>
                        <div class="col-md-3 "  ng-switch on="item.joinGangState">
                            <span ng-switch-when="0">已入伙</span>
                            <span ng-switch-when="1">没入伙</span>
                        </div>
                        <div class="col-md-2 text-right">竣工面积：</div>
                        <div class="col-md-2 ">{{item.houseArea}}</div>
                        <div class="col-md-2 text-right">房屋户型：</div>
                        <div class="col-md-2 ">{{item.houseName}}</div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<div ng-show="show2">
<div class="row padding">
<div class="col-lg-12">
    <!--房屋地址表格-->
<div class="row search-padding-10">
    <div class="col-lg-6 col-md-6 col-sm-6">
        房屋地址：{{hose.fullName}}
    </div>
    <div class="col-lg-6 col-md-6 col-sm-6 text-right">
        可加建范围：无
    </div>
</div>
    <table >
        <thead>
        <tr>
            <th>装修图纸管理</th>
            <th>户型图</th>
            <th>水路图</th>
            <th>电路图</th>
        </tr>
        </thead>
        <tbody>
        <tr>
            <td>原图</td>
            <td><a class="glyphicon glyphicon-plus" data-toggle="modal" data-target="#ddd" style="cursor:pointer"  ng-click="OwnerRenovationApplyDrawing()"></a></td>
            <td><a class="glyphicon glyphicon-plus" data-toggle="modal" data-target="#ddd" style="cursor:pointer"  ng-click="OwnerRenovationApplyDrawing1()"></a></td>
            <td><a class="glyphicon glyphicon-plus" data-toggle="modal" data-target="#ddd" style="cursor:pointer"  ng-click="OwnerRenovationApplyDrawing2()"></a></td>
        </tr>
        <tr>
            <td>设计图</td>
            <td><a class="glyphicon glyphicon-plus" data-toggle="modal" data-target="#ddd" style="cursor:pointer"  ng-click="OwnerRenovationApplyDrawingT()"></a></td>
            <td><a class="glyphicon glyphicon-plus" data-toggle="modal" data-target="#ddd" style="cursor:pointer"  ng-click="OwnerRenovationApplyDrawingT1()"></a></td>
            <td><a class="glyphicon glyphicon-plus" data-toggle="modal" data-target="#ddd" style="cursor:pointer"  ng-click="OwnerRenovationApplyDrawingT2()"></a></td>
        </tr>
        </tbody>
    </table>
    <!--房屋地址表格 结束-->
    <!--装修对象表格-->
    <div class="search-padding-10">
            装修对象选择：
    </div>
    <table>
        <thead>
        <tr>
            <th>建筑组建名称</th>
            <th>建筑组建编号</th>
            <th>选择</th>
        </tr>
        </thead>
        <tbody>
        <tr ng-repeat="com in componentproperty">
            <td>{{com.functionalName}}</td>
            <td>{{ buildingComponent.componentNum }}</td>
            <td>
                <input type="checkbox" style="width:15px;height: 15px;">
            </td>
        </tr>
        <tr>
            <td colspan="3">全部装修</td>
        </tr>
        </tbody>
    </table>
    <!--装修对象表格 结束-->
    <!--条件搜索-->
    <form id="css_form" name="mainForm" novalidate>
    <div class="row sel1">
        <div class="col-lg-1 col-md-2 col-sm-2 text-right">装修类型：</div>
        <div class="col-lg-1 col-md-2 col-sm-2">
            <label class="radio inline">
                <input type="radio" name="1" value="0" ng-model="OwnerRenovationApply.decorateType">
                <span>自装</span>
            </label>
        </div>
        <div class="col-lg-2 col-md-2 col-sm-2">
            <label class="radio inline">
                <input type="radio" name="1" value="1" ng-model="OwnerRenovationApply.decorateType">
                <span>工装</span>
            </label>
        </div>
        <div class="col-lg-1 col-md-2 col-sm-2 text-right">拆墙面积：</div>
        <div class="col-lg-3 col-md-3 col-sm-3">
            <input class="form-control"ng-model="OwnerRenovationApply.actualDismantlesWallArea"name="actualDismantlesWallArea"  ng-pattern="/^[0-9]*$/"required  placeholder="请输入">
            <span class="spanInfo" ng-show="mainForm.actualDismantlesWallArea.$dirty && mainForm.actualDismantlesWallArea.$invalid">
                <span ng-show="mainForm.actualDismantlesWallArea.$error.required" style="color: red">* 不能为空</span>
                <span ng-show="mainForm.actualDismantlesWallArea.defined ">* 正确</span>
                <span ng-show="mainForm.actualDismantlesWallArea.$error.pattern" style="color: red">* 只能是数字</span>
            </span>
        </div>
        <div class="col-lg-4 col-md-1 col-md-1" style="height:40px">m2</div>
        <div class="col-lg-1 col-md-2 col-sm-2 text-right">装修单位：</div>
        <div class="col-lg-3 col-md-4 col-sm-4">
            <select class="form-control" ng-options="option.enterpriseId as option.enterpriseName for option in enterpriseCustNew" ng-model="OwnerRenovationApply.decorateAssociatedRecords.decorateId" ng-change="change11(OwnerRenovationApply.decorateAssociatedRecords.decorateId)">
                <option value="">--请选择--</option>
            </select>
        </div>
        <div class="col-lg-1 col-md-2 col-sm-2 text-right">负责人：</div>
        <div class="col-lg-3 col-md-4 col-sm-4">
            <select class="form-control" ng-options="option.custId as option.name for option in PersonCustNew" ng-model="OwnerRenovationApply.decorateAssociatedRecords.headId">
                <option value="">--请选择--</option>
            </select>
        </div>
        <div class="col-lg-4 col-md-6 col-sm-6">公司法人：{{EnterpriseCustNew.representative}}</div>
    </div>
    </form>
    <!--条件搜索 结束-->
    <!--装修人员-->
    <div class="search-padding-10">
        现场  装修  人员：
    </div>
    <table>
        <thead>
        <tr>
            <th>装修人员姓名</th>
            <th>装修人员电话</th>
        </tr>
        </thead>
        <tbody>
        <tr>
            <td>{{itte.name}}</td>
            <td>{{itte.phoneNum}}</td>
        </tr>
        </tbody>
    </table>
    <!--装修人员 结束-->
    <!--装修单位-->
    <div class="search-padding-10">
        装修单位提交资料：
    </div>
    <table>
        <thead>
        <tr>
            <th></th>
            <th>消防批文</th>
            <th>装修图纸</th>
            <th>动火协议</th>
            <th>合同文件</th>
            <th>其他</th>
        </tr>
        </thead>
        <tbody>
        <tr>
            <td>文件</td>
            <td><a class="glyphicon glyphicon-plus" data-toggle="modal" data-target="#ddd" style="cursor:pointer"  ng-click="FiledApply()"></a></td>
            <td><a class="glyphicon glyphicon-plus" data-toggle="modal" data-target="#ddd" style="cursor:pointer"  ng-click="FiledApply1()"></a></td>
            <td><a class="glyphicon glyphicon-plus" data-toggle="modal" data-target="#ddd" style="cursor:pointer"  ng-click="FiledApply2()"></a></td>
            <td><a class="glyphicon glyphicon-plus" data-toggle="modal" data-target="#ddd" style="cursor:pointer"  ng-click="FiledApply3()"></a></td>
            <td><a class="glyphicon glyphicon-plus" data-toggle="modal" data-target="#ddd" style="cursor:pointer"  ng-click="FiledApply4()"></a></td>
        </tr>
        </tbody>
    </table>
    <!--装修单位  结束-->
    <div class="row search-padding-10">
        <div class="col-lg-2 col-md-2 col-sm-2 text-right">装修预计开始时间：</div>
        <div class="col-lg-4 col-md-4 col-sm-4">
            <input type="datetime-local" ng-model="OwnerRenovationApply.estimatedStarTime" >
        </div>
        <div class="col-lg-2 col-md-2 col-sm-2 text-right">装修预计结束时间：</div>
        <div class="col-lg-4 col-md-4 col-sm-4">
            <input type="datetime-local" ng-model="OwnerRenovationApply.estimatedFinishTime" >
        </div>
    </div>

    <!--下一步按钮-->
    <div class="text-center">
        <button type="button" class="btn btn-default btn-primary" >
            <span class="glyphicon glyphicon-remove btn_icon"></span>
            取消
        </button>
        <button type="button" class="btn btn-default btn-primary" ng-click="addDecorate()" >
            <span class="glyphicon glyphicon-saved btn_icon"></span>
            创建装修申请服务及任务
        </button>
    </div>
    <!--下一步按钮  结束-->
</div>
</div>
</div>
<div ng-show="show3">
    <div class="complaint_user_list check_box"data-toggle="tab" href="#">
        <div class="text_top">
            <span   data-target="#addProjectOne1"  data-toggle="modal" style="cursor:pointer" ng-click="SelectHeCha()">等待核查任务完成</span>
        </div>
    </div>
    <div class="complaint_user_list check_box" data-toggle="tab" href="#">
        <div class="text_top">
            <span data-target="#addProject1"  data-toggle="modal" style="cursor:pointer">填写核查结果</span>
        </div>
    </div>
    <!--填写结果模态框-->
    <div class="modal fade" id="addProject1" role="dialog" aria-label="myModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    填写结果
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class=" padding">
                        <div class="row search-padding-10">
                            <div class="col-lg-6 col-md-6 col-sm-6">负责人：老王</div>
                            <div class="col-lg-6 col-md-6 col-sm-6">预约时间：2015/5/5</div>
                        </div>
                        <div class="row search-padding-10">
                            <div class="col-lg-6 col-md-6 col-sm-6">核查单</div>
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <button type="button" class="btn btn-default btn-primary" ng-click="addItem()">
                                    <span class="glyphicon glyphicon-plus btn_icon"></span>
                                    新建检查项
                                </button>
                            </div>
                        </div>
                        <table>
                            <thead>
                            <tr>
                                <th>序号</th>
                                <th>是否进行(默认不进行)</th>
                                <th>是否异常(默认正常)</th>
                                <th>操作</th>
                            </tr>
                            </thead>
                            <tbody>
                            <tr ng-repeat="item in currentBuilding.areaNew">
                                <td>{{item.inspectName}}</td>
                                <td><input type="radio" class="checkbox_info" value="1" ng-model="item.conduct">进行</td>
                                <td><input type="radio" class="checkbox_info" value="1" ng-model="item.normal">异常</td>
                                <td>
                                    <input type="button"  class="modify_button"  ng-click="updateItem($index)" value="修改">
                                    <input type="button"  class="del_button"  ng-click="deleteItem($index)" value="删除">
                                </td>
                            </tr>
                            <tr ng-show="editArea">
                                <td><input ng-model="currentEditArea.inspectName"></td>
                                <td>
                                    <select  ng-model="currentEditArea.conduct">
                                        <option value="">-- 请选择 --</option>
                                        <option value="0">进行</option>
                                    </select>
                                </td>
                                <td>
                                    <select ng-model="currentEditArea.normal">
                                        <option value="">-- 请选择 --</option>
                                        <option value="0">异常</option>
                                    </select>
                                </td>
                                <td >
                                    <button type="button" ng-click="addSave()">保存</button>
                                    <button type="button" ng-click="addCancel()">取消</button>
                                </td>
                            </tr>
                            </tbody>
                        </table>
                        <div class="search-padding-10">装修设计图是否合理</div>
                        <table>
                            <thead>
                            <tr>
                                <th>设计图</th>
                                <th>图纸</th>
                                <th>是否合理</th>
                            </tr>
                            </thead>
                            <tbody>
                            <tr>
                                <td>装修设计图</td>
                                <td><img></td>
                                <td><input type="radio" class="checkbox_info" value="0" ng-model="RenovationCheckOrders.architecturalDesign">不合理</td>
                            </tr>
                            <tr>
                                <td>水路图</td>
                                <td><img></td>
                                <td><input type="radio" class="checkbox_info" value="0" ng-model="RenovationCheckOrders.waterPipeDesign">不合理</td>
                            </tr>
                            <tr>
                                <td>电路图</td>
                                <td><img></td>
                                <td><input type="radio" class="checkbox_info" value="0" ng-model="RenovationCheckOrders.circuitDesign">不合理</td>
                            </tr>
                            </tbody>
                        </table>
                        <div class="search-padding-10">是否建议发函</div>
                        <table>
                            <thead>
                            <tr>
                                <th>函件名</th>
                                <th>是否建议发函</th>
                            </tr>
                            </thead>
                            <tbody>
                            <tr ng-repeat="vms in currentLetter">
                                <td>{{vms.letterName}}</td>
                                <td><input type="radio" class="checkbox_info" value="1" ng-model="vms.whetherLetter">建议发函</td>
                            </tr>
                            </tbody>
                        </table>
                        <div class="row search-padding-10">
                            <div class="col-lg-3 col-md-3 col-sm-3">
                                是否继续装修
                            </div>
                            <div class="col-lg-2 col-md-2 col-sm-2">
                                <label class="radio inline">
                                    <input name="name1" type="radio" value="0" ng-model="RenovationCheckOrders.whetherToContinue">
                                    <span>不可以</span>
                                </label>
                            </div>
                            <div class="col-lg-2 col-md-2 col-sm-2">
                                <label class="radio inline">
                                    <input name="name2" type="radio" value="1" ng-model="RenovationCheckOrders.whetherToContinue">
                                    <span>可以</span>
                                </label>
                            </div>
                        </div>
                        <div class="search-padding-10">备注</div>
                        <div class="search-padding-10">
                            <textarea class="text_min" ng-model="RenovationCheckOrders.remarks"></textarea>
                        </div>
                        <div class="search-padding-10">
                            <button class="btn btn-primary" role="presentation" data-toggle="modal" data-target="#ddd" style="cursor:pointer" data-toggle="tab" ng-click="constructionAdd()">上传</button>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                        <button href="" class="btn btn-primary" data-target="#addProjectOne1"  data-toggle="modal" style="cursor:pointer"data-dismiss="modal" ng-click="addHeCha(RenovationCheckOrders.taskId)">确定</button>
                        <button href="" class="btn btn-default" data-dismiss="modal">取消</button>
                </div>
            </div>
        </div>
    </div>
    <!--填写结果模态框结束-->
    <!--填写结果确定模态框-->
    <div class="modal fade" id="addProjectOne1" role="dialog" aria-label="myModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    结果
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="padding">
                        <div class="row search-padding-10">
                            <div class="col-lg-6 col-md-6 col-sm-6 ">负责人：老王</div>
                            <div class="col-lg-6 col-md-6 col-sm-6 text-right">预约时间：2015/5/5</div>
                        </div>
                        <div class="search-padding-10">
                            核查单
                        </div>
                        <table>
                            <thead>
                            <tr>
                                <th>序号</th>
                                <th>是否进行</th>
                                <th>是否异常</th>
                            </tr>
                            </thead>
                            <tbody>
                            <tr ng-repeat="vm in RenovationCheckOrders.renovationCheckRecords">
                                <td>{{vm.inspectName}}</td>
                                <td>
                                    <ul class="receive_list">
                                        <li ng-switch on="vm.conduct">
                                            <b  ng-switch-when="0">不进行</b>
                                            <b ng-switch-when="1">进行</b>
                                        </li>
                                    </ul>
                                </td>
                                <td >
                                    <ul class="receive_list">
                                        <li ng-switch on="vm.normal">
                                            <b  ng-switch-when="0">正常</b>
                                            <b ng-switch-when="1">异常</b>
                                        </li>
                                    </ul>
                                </td>
                            </tr>
                            </tbody>
                        </table>
                        <div class="search-padding-10">
                            装修设计图是否合理
                        </div>
                        <table>
                       <thead>
                            <tr>
                                <th>设计图</th>
                                <th>图纸</th>
                                <th>是否合理</th>
                            </tr>
                       </thead>
                            <tbody>
                            <tr>
                                <td>装修设计图</td>
                                <td><img></td>
                                <td>
                                    <ul class="receive_list">
                                        <li ng-switch on="RenovationCheckOrders.architecturalDesign">
