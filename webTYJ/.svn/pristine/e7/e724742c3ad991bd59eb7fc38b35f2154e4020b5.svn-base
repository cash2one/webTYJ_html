                <!--<div><span>共{{searchPaginator.object.page.totalResult}}条</span></div>-->
            </div>
        </div>
        <div class="col-md-8" ng-show="showInternalTag==1">
            <div class="col-md-12" style="border-bottom:2px solid #cccccc">
                <div class="col-md-12"><h5><strong>{{itemInfoInternal.inform}}</strong></h5></div>
                <div class="col-md-12">
                    <div class="col-md-5">发布单位：深圳翔恒科技开发有限公司</div>
                    <div class="col-md-3 text-right">发布者：{{itemInfoInternal.writer}}</div>
                    <div class="col-md-4 text-right">{{itemInfoInternal.time | date:"yyyy-MM-dd"}}</div>
                </div>
            </div>
            <div class="col-md-12">
                <div class="col-md-12" id="textInternal">
                    <!--{{itemInfoCommunity.briefContent}}-->
                </div>
            </div>
        </div>
    </div>
</div>
</body>
</html>