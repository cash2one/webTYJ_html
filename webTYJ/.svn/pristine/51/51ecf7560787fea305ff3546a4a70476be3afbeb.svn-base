<!DOCTYPE html>
<html>
<head lang="en">
    <meta charset="UTF-8">
    <title></title>
    <link rel="stylesheet" href="bower_components/skins/zyupload-1.0.0.min.css " type="text/css">

    <link rel="stylesheet" href="css/ztree/demo.css" type="text/css">
    <link rel="stylesheet" href="css/ztree/zTreeStyle.css" type="text/css">
    <script type="text/javascript" src="bower_components/ztree/jquery.ztree.core-3.5.js"></script>
    <script type="text/javascript" src="bower_components/ztree/jquery.ztree.excheck-3.5.js"></script>
    <script type="text/javascript" src="bower_components/ztree/jquery.ztree.exedit-3.5.js"></script>
    <script type="text/javascript">
        function exportExcelWater() {
            var url = document.getElementById("fileurl").value;
            window.location.href = url + "/info/outWaterMeterExcel.do?"
        }

    </script>
</head>
<body>
<form id="css_form" name="mainForm" novalidate>
    <!--搜索-->
    <div class="sel_top" ng-show="show8">
        <div class="row">
            <div class="col-lg-9 col-md-9 col-sm-6">
                <button type="button" class="btn btn-default btn-primary" data-target="#outPut" data-toggle="modal"
                        ng-click="inportExcel()">
                    <span class="glyphicon glyphicon-log-in btn_icon"></span>
                    导入
                </button>
                <button type="button" class="btn btn-default btn-primary" data-target="#new" data-toggle="modal">
                    <span class="glyphicon glyphicon-plus btn_icon"></span>
                    新建
                </button>
                <button type="button" class="btn btn-default btn-primary" ng-click="updateWater(item2)">
                    <span class="glyphicon glyphicon-edit btn_icon"></span>
                    编辑
                </button>
                <button type="button" class="btn btn-default btn-primary" ng-click="showLink(item2)">
                    <span class="glyphicon glyphicon-link btn_icon"></span>
                    关联
                </button>
                <button type="button" class="btn btn-default btn-primary" ng-click="disableWater(item2)">
                    <span class="glyphicon glyphicon-ban-circle btn_icon"></span>
                    停用
                </button>
            </div>
            <div class="col-lg-2 col-md-2 col-sm-4"><input class="form-control" ng-model="searchMeter.waterMeterNum">
            </div>
            <div class="col-lg-1 col-md-1 col-sm-2">
                <button type="button" class="btn btn-default btn-primary" ng-click="currentWater._load()">
                    <span class="glyphicon glyphicon-search btn_icon"></span>
                    查询
                </button>
            </div>
        </div>
    </div>
    <!--搜素结束-->
    <div class="row padding" ng-show="show9">
        <div class="col-lg-12">
            <table>
                <thead>
                <tr>
                    <th width="50px"></th>
                    <!--<th>序号</th>-->
                    <th>资产编号</th>
                    <th>水表编号</th>
                    <th>水表状态</th>
                    <th>区域</th>
                </tr>
                </thead>
                <tbody>
                <tr ng-dblclick="showList(item)" ng-repeat="item in currentWater.object['waterMeter']"
                    ng-show="item['waterMeterId'] != null" ng-class="{'this_info':btnIndex==item}"
                    ng-click="checkCurrentWater(item)">
                    <!--<td><input type="checkbox"></td>-->
                    <td>{{$index}}</td>
                    <td>{{item.propertyNumber}}</td>
                    <td>{{item.waterMeterNum}}</td>
                    <td>
                        <div ng-switch on="{{item['state']}}">
                            <div class="animate-switch" ng-switch-when="1">新建</div>
                            <div class="animate-switch" ng-switch-when="4">未使用</div>
                            <div class="animate-switch" ng-switch-when="2">使用中</div>
                            <div class="animate-switch" ng-switch-when="3">停用</div>
                        </div>
                    </td>
                    <td>{{item.fullName}}</td>
                </tr>
                </tbody>
            </table>
            <div class="bill_paging">
                <ul class="pagination">
                    <li><a style="cursor:pointer;" ng-click="currentWater.go(1)">首页</a></li>
                    <li><a style="cursor:pointer;" ng-show="currentWater.hasPrevious()"
                           ng-click="currentWater.previous()">上一页</a></li>
                    <li><a style="cursor:pointer;" ng-repeat="i in currentWater.pages" ng-click="currentWater.go(i)">{{i}}</a>
                    </li>
                    <li><a style="cursor:pointer;" ng-show="currentWater.hasNext()"
                           ng-click="currentWater.next()">下一页</a></li>
                    <li><a style="cursor:pointer;" ng-click="currentWater.go(currentWater.page.totalPage)">尾页</a></li>
                </ul>
            </div>
        </div>
    </div>
    <!--新建-->
    <div class="modal fade" id="new" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span
                            aria-hidden="true">&times;</span></button>
                    <span class="modal-title">新建</span>
                </div>
                <div class="modal-body">
                    <div class="padding">
                        <div class="row sel1">
                            <div class="col-lg-2 col-md-2 col-sm-2 text-right">资产名称：</div>
                            <div class="col-lg-3 col-md-4 col-sm-4"><input class="form-control"
                                                                           ng-model="currentAddWater.property.propertyName"
                                                                           name="propertyName"
                                                                           placeholder="请输入资产名称">
                                       <span class="glyphicon glyphicon-exclamation-sign mess"
                                             ng-show="mainForm.propertyName.$dirty && mainForm.propertyName.$invalid">
                                                         <p class="alter"
                                                            ng-show="mainForm.propertyName.$error.required">
                                                             不能为空
                                                         </p>
                                                        <p class="alter" ng-show="mainForm.propertyName.defined ">
                                                            正确
                                                        </p>
                                                  </span>
                            </div>
                            <div class="col-lg-1 col-md-2 col-sm-2 "></div>
                            <div class="col-lg-2 col-md-2 col-sm-2 text-right">资产编号：</div>
                            <div class="col-lg-3 col-md-4 col-sm-4"><input class="form-control"
                                                                           ng-model="currentAddWater.property.propertyNumber"
                                                                           ng-pattern="/^[0-9]*$/" name="propertyNumber"
                                                                           placeholder="请输入数字">
                                   <span class="glyphicon glyphicon-exclamation-sign mess"
                                         ng-show="mainForm.propertyNumber.$dirty && mainForm.propertyNumber.$invalid">
                                                         <p class="alter"
                                                            ng-show="mainForm.propertyNumber.$error.required">
                                                             不能为空
                                                         </p>
                                                        <p class="alter" ng-show="mainForm.propertyNumber.defined ">
                                                            正确
                                                        </p>
                                                  </span>
                            </div>
                            <div class="col-lg-1 col-md-2 col-sm-2 "></div>
                            <!-- <div class="col-lg-2 col-md-2 col-sm-2 text-right">操作人：</div>
                             <div class="col-lg-3 col-md-4 col-sm-4"><input class="form-control"
                                                                            ng-model="currentAddWater.operator"
                                                                            name="operator"
                                                                            placeholder="请输入操作人">
                                    <span class="glyphicon glyphicon-exclamation-sign mess"
                                          ng-show="mainForm.operator.$dirty && mainForm.operator.$invalid">
                                                          <p class="alter" ng-show="mainForm.operator.$error.required">
                                                              不能为空
                                                          </p>
                                                         <p class="alter" ng-show="mainForm.operator.defined ">
                                                             正确
                                                         </p>
                                                   </span>
                             </div>
                             <div class="col-lg-1 col-md-2 col-sm-2 "></div>-->
                            <div class="col-lg-2 col-md-2 col-sm-2 text-right">保质期：</div>
                            <div class="col-lg-3 col-md-4 col-sm-4"><input class="form-control"
                                                                           ng-model="currentAddWater.property.expirationDate"
                                                                           name="expirationDate"
                                                                           placeholder="请输入保质期">
                                   <span class="glyphicon glyphicon-exclamation-sign mess"
                                         ng-show="mainForm.expirationDate.$dirty && mainForm.expirationDate.$invalid">
                                                         <p class="alter"
                                                            ng-show="mainForm.expirationDate.$error.required">
                                                             不能为空
                                                         </p>
                                                        <p class="alter" ng-show="mainForm.expirationDate.defined ">
                                                            正确
                                                        </p>
                                                  </span>
                            </div>
                            <div class="col-lg-1 col-md-2 col-sm-2 "></div>
                            <div class="col-lg-2 col-md-2 col-sm-2 text-right">产地：</div>
                            <div class="col-lg-3 col-md-4 col-sm-4"><input class="form-control"
