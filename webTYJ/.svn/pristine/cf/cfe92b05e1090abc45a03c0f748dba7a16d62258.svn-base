<!--客户查询列表 杨升权 2016/7/21-->
<div class="modal fade" id="myModalPerson" tabindex="-1"
     role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog" >
        <div class="modal-content" style="width: 700px">
            <div class="modal-header">
                <span>人员信息</span>
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>

            </div>
            <div class="modal-body">
                <div class="sel">
                    <div class="col-2 col-md-2 col-sm-2 text-right">
                        姓名:
                    </div>
                    <div class="col-4 col-md-4 col-sm-4">
                        <input placeholder="请输入姓名" type="text" class="form-control " ng-model="searchone.staffName">
                    </div>
                    <div class="col-2 col-md-2 col-sm-2 text-right">
                        联系电话:
                    </div>
                    <div class="col-4 col-md-4 col-sm-4 ">
                        <input type="text" placeholder="请输入联系电话" class="form-control " ng-model="searchone.phone">
                    </div>
                    <div class="col-2 col-md-2 col-sm-2 text-right">
                        身份证号
                    </div>
                    <div class="col-4 col-md-4 col-sm-4">
                        <input type="text" placeholder="请输入身份证号" class="form-control" ng-model="searchone.idNum">
                    </div>

                    <div class="col-6 col-md-6 col-sm-6 text-right">
                        <button type="button" class="btn btn-default btn-primary tab-btn" ng-click="currentPaginator._load();">
                            <span class="glyphicon glyphicon-search btn_icon"></span>
                            查询
                        </button>
                    </div>

                    <div class="clear"></div>

                </div>
                <div class="padding">
                    <table class="table_lists" ng-repeat="person in currentPaginator.object.personCustNew" ng-show="person.custId!=null" >
                        <tbody ng-class="{'this_info ':btnIndex==person['custId']}"   ng-click="choicePerson1(person)" >
                        <tr>
                            <td width="50px" rowspan="2"><img src="images/propertyService/choiceHousing/u81.jpg" ng-click="choicePerson1(person)" width="50" height="50" ></td>
                            <td>客户姓名：{{person.name}}</td>
                            <td>性别：{{person.sex}}</td>
                            <td>家家号：{{person.jiajiaNum}}</td>
                            <td>身份证：{{person.cardNum}}</td>
                        </tr>
                        <tr>
                            <td>客户编号：{{person.custCode}}</td>
                            <td>年龄：{{person.age}}</td>
                            <td>联系电话：{{person.phoneNum}}</td>
                            <td>QQ号码：{{person.weixinNum}}</td>
                        </tr>
                        </tbody>
                    </table>

                    <div class="padding" ng-show="currentPaginator.object.page.totalResult!=0">
                        <div class="bill_paging">
                            <span>共{{currentPaginator.object.page.totalResult}}条</span>
                            <ul class="pagination">
                                <li ng-class="{true:'disabled'}[currentPaginator.page.currentPage==1]">
                                    <a href="javascript:void(0)" ng-click="currentPaginator.go(1)">首页</a>
                                </li>
                                <li ng-class="{true:'disabled'}[currentPaginator.page.currentPage==1]">
                                    <a href="javascript:void(0)" ng-show="currentPaginator.hasPrevious()" ng-click="currentPaginator.previous()">上一页</a>
                                </li>
                                <li ng-class="{true:'active'}[currentPaginator.page.currentPage==i]" ng-repeat="i in currentPaginator.pages">
                                    <a href="javascript:void(0)"  ng-click="currentPaginator.go(i)">{{i}}</a>
                                </li>
                                <li ng-class="{true:'disabled'}[currentPaginator.page.currentPage==currentPaginator.page.totalPage]">
                                    <a href="javascript:void(0)" ng-show="currentPaginator.hasNext()" ng-click="currentPaginator.next()">下一页</a>
                                </li>
                                <li ng-class="{true:'disabled'}[currentPaginator.page.currentPage==currentPaginator.page.totalPage]">
                                    <a href="javascript:void(0)" ng-click="currentPaginator.go(currentPaginator.page.totalPage)">尾页</a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>

            <div class="modal-footer">
                <button type="button" class="btn  btn-default" ng-click="subCancel()" data-dismiss="modal">
                    取消
                </button>
                <button type="button" class="btn bg-primary" aria-label="Left Align" ng-click="subSave()" data-dismiss="modal">
                    确定
                </button>
            </div>
        </div>
    </div>
</div>
</body>
</html>