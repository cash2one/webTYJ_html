<div class="row" ng-show="show4">
    <div class="row">
        <div class="complaint_user_list building_padding">
            <!--收费表格-->
            <div class="table">
                <table width="100%">
                    <tr class="table_nav">
                        <td>收费编码</td>
                        <td>收费名称</td>
