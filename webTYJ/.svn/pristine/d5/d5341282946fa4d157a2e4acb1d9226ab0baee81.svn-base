<!--页面名称：物业服务办理装修流程页面-->
<!DOCTYPE html>
<html>
<head lang="en">
    <meta charset="UTF-8">
    <title>办理装修流程</title>
    <link rel="stylesheet" href="css/propertyService.css">

    <style type="text/css">
        #pm img{width:80px;height:80px;}
        .doted{ height:20px; text-align:center; width:80px; margin:0 auto; }
        .doted .dot_unChecked{ display:block; width:25px; height:25px; background:#e5e5e7; border:1px solid #a4a4a4; -moz-border-radius:50%; -webkit-border-radius:50%; border-radius:50%; float:left; margin-left:50px; margin-top:10px;}
        .doted .dot_Check{ display:block; width:25px; height:25px; background:#02a0e7; -moz-border-radius:50%; -webkit-border-radius:50%; border-radius:50%; float:left; margin-left:50px; margin-top:10px;}
        .doted .dot_Check{ display:block; width:25px; height:25px; background:#02a0e7; -moz-border-radius:50%; -webkit-border-radius:50%; border-radius:50%; float:left; margin-left:50px; margin-top:10px;}
        .doted .dot_unChecked:hover{ display:block; width:25px; height:25px; background:#02a0e7; -moz-border-radius:50%; -webkit-border-radius:50%; border-radius:50%; float:left; margin-left:50px; margin-top:10px;}
        .connectLines hr{width:76px;text-align: center;border: 1px dotted rgb(180,180,180);position: absolute;margin: 0 0 0 76px;}
    </style>
</head>
<body>

<div class="complaint_user" >
    <div class="row col-md-12" style="margin-left: 43px;margin-top: 5px;text-align: center;">
        <div class="col-md-1" style="margin-left: 15px">
            <span>选择房屋</span>
        </div>
        <div class="col-md-1" style="margin-left: 25px">
            <span>申请资料</span>
        </div>
        <div class="col-md-1" style="margin-left: 25px">
            <span>现场核查</span>
        </div>
        <div class="col-md-1" style="margin-left: 25px">
            <span>缴费</span>
        </div>
        <div class="col-md-1" style="margin-left: 25px">
            <span>发放许可</span>
        </div>
        <div class="col-md-1" style="margin-left: 25px">
            <span>施工过程</span>
        </div>
        <div class="col-md-1" style="margin-left: 25px">
            <span>验收</span>
        </div>
        <div class="col-md-1" style="margin-left: 25px">
            <span>费用结算</span>
        </div>
    </div>
    <div class="row col-md-12" style="margin-left: -4px;margin-bottom: 20px;">
        <div class="col-md-1" style="margin-left: 25px">
                <span>
                    <div class="doted" style="cursor:pointer">
                        <a><span class="dot_Check" ng-click="chooseHouse()"></span></a>
                    </div>
                    <div class="connectLines">
                        <hr>
                    </div>
                </span>
        </div>
        <div class="col-md-1" style="margin-left: 25px">
                <span>
                    <div class="doted" style="cursor:pointer">
                        <a><span class="dot_unChecked" ng-click="applicationData()"></span></a>
                    </div>
                    <div class="connectLines">
                        <hr>
                    </div>
                </span>
        </div>
        <div class="col-md-1" style="margin-left: 25px">
                <span>
                    <div class="doted" style="cursor:pointer">
                        <a><span class="dot_unChecked" ng-click="verification()"></span></a>
                    </div>
                    <div class="connectLines">
                        <hr>
                    </div>
                </span>
        </div>
        <div class="col-md-1" style="margin-left: 25px">
                <span>
                    <div class="doted" style="cursor:pointer">
                        <a><span class="dot_unChecked" ng-click="payDeposit()"></span></a>
                    </div>
                    <div class="connectLines">
                        <hr>
                    </div>
                </span>
        </div>
        <div class="col-md-1" style="cursor:pointer;margin-left: 25px">
                <span>
                    <div class="doted">
                        <a><span class="dot_unChecked" ng-click="Alicence()"></span></a>
                    </div>
                    <div class="connectLines">
                        <hr>
                    </div>
                </span>
        </div>
        <div class="col-md-1" style="cursor:pointer;margin-left: 25px">
                <span>
                    <div class="doted">
                        <a><span class="dot_unChecked" ng-click="workProgress()"></span></a>
                    </div>
                    <div class="connectLines">
                        <hr>
                    </div>
                </span>
        </div>
        <div class="col-md-1" style="cursor:pointer;margin-left: 25px">
                <span>
                    <div class="doted">
                        <a><span class="dot_unChecked" ng-click="acceptance()"></span></a>
                    </div>
                    <div class="connectLines">
                        <hr>
                    </div>
                </span>
        </div>
        <div class="col-md-1" style="margin-left: 25px">
                <span>
                    <div class="doted">
                        <a><span class="dot_unChecked" ng-click="costClearing()"></span></a>
                    </div>
                </span>
        </div>
    </div>
    <div class="clear"></div>
</div>
<div class="row" ng-show="show1">
    <div class="row">
        <div class="complaint_user rightpadding">
            <!--业主信息-->
            <div class="architecture_table_padding">
                <div class="row padding_lease"  >
                    <div class="lease_search_height overflow_y">
                        <div class="info_lista" ng-repeat="item in HouseNew">
                            <div class="media-left">
                                <a href="#">
                                    <img src="images/propertyService/choiceHousing/u81.jpg" width="50px;" height="50px" style="margin-top: 5px;">
                                </a>
                            </div>
                            <div class="media-body" >
                                <div class="row">
                                    <div class="col-md-6">房屋地址：{{item.houseAddress}}</div>
                                    <div class="col-md-3">业主姓名：{{user.name}}</div>
                                </div>
                                <div class="row">
                                    <div class="col-md-3">房屋编码：{{item.houseNum}}</div>
                                    <div class="col-md-3"  ng-switch on="item.joinGangState">入伙状态：
                                        <span ng-switch-when="0">已入伙</span>
                                        <span ng-switch-when="1">没入伙</span>
                                    </div>
                                    <div class="col-md-3">竣工面积:{{item.houseArea}}</div>
                                    <div class="col-md-3">房屋户型:{{item.houseName}}</div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!--业主信息 结束-->

            <!--下一步按钮-->
            <div class="save_concel_botton contract_detail_botton">
                <a href="" class="radius_all button_bule">取消</a>
                <a href="" class="radius_all button_bule">下一步</a>
                <div class="clear"></div>
            </div>
            <!--下一步按钮 结束-->

        </div>
    </div>
</div>
<div class="row" ng-show="show2">
    <div class="row">
        <div  class="complaint_user rightpadding"  >
