                </button>
                <button type="button" class="btn btn-default btn-primary" ng-click="clearAll()">
                    清空
                </button>
            </div>
        </div>
            
        <div class="search-padding-10">
            <table>
                <thead>
                <tr>
                    <th width="50px"></th>
                    <th>项目</th>
                    <th>代办银行</th>
                    <th>收付单位代码</th>
                    <th>币种</th>
                    <th>收付标志</th>
                    <th>收付代码</th>
                    <th>交易文件</th>
                    <th>总户数</th>
                    <th>总金额</th>
                    <th>已划款户数</th>
                    <th>已划款金额</th>
                    <th>送银行日期</th>
                    <th>应划款日期</th>
                    <th>划款日期</th>
                </tr>
                </thead>
                <tbody>
                <tr ng-repeat="item in financialGroupTotal" ng-click="importDetails(item.financialGroupTotalId)" data-toggle="modal" data-target="#ddd" data-toggle="tab"  ng-class="{'this_info':btnIndex2==item.financialGroupTotalId}">
                    <td><input type="checkbox"></td>
                    <td>{{item.projectName}}</td>
                    <td>{{item.bankType}}</td>
                    <td>{{item.bankNum}}</td>
                    <td ng-switch on="item.coinType">
                        <span ng-switch-when="0">人民币</span>
                        <span ng-switch-when="1">港币</span>
                    </td>
                    <td ng-switch on="item.paymentState">
                        <span ng-switch-when="0">已收</span>
                        <span ng-switch-when="1">未收</span>
                    </td>
                    <td>{{item.collectionTotalNum}}</td>
                    <td>{{item.collectionTotalName}}</td>
                    <td>{{item.houseNumber}}</td>
                    <td>{{item.sum}}</td>
                    <td>{{item.deductionsHouseNumber}}</td>
                    <td>{{item.deductionsSum}}</td>
                    <td>{{item.tobankDate|date:'yyyy-MM-dd'}}</td>
                    <td>{{item.prepareDeductionsDate|date:'yyyy-MM-dd'}}</td>
                    <td>{{item.deductionsDate|date:'yyyy-MM-dd'}}</td>
                </tr>
                </tbody>
            </table>
        </div>
        <div class="search-padding-10">
            明细文件
        </div>
        <div class="search-padding-10">
            <table>
                <thead>
                <tr>
                    <th>合同号</th>
                    <th>行别 </th>
                    <th>开户行号</th>
                    <th>托收管理id</th>
                    <th>账单id</th>
                    <th>扣款标记</th>
                    <th>托收状态</th>
                    <th>生成日期</th>
                    <th>托收类型</th>
                    <th>项目id</th>
                </tr>
                </thead>
                <tbody>
                <tr ng-repeat="item in financialGroupDetails">
                    <td>{{item.contractNumber}}</td>
                    <td>{{item.bankType}}</td>
                    <td>{{item.bankNum}}</td>
                    <td>{{item.collectionManegeId}}</td>
                    <td>{{item.billId}}</td>
                    <td>{{item.deductionsTag}}</td>
                    <td ng-switch on="item.collectionState">
                        <span ng-switch-when="0">未托收</span>
                        <span ng-switch-when="1">已托收</span>
                    </td>
                    <td>{{item.birthDate | date : 'yyyy-MM-dd'}}</td>
                    <td ng-switch on="item.collectionType">
                        <span ng-switch-when="0">托收</span>
                        <span ng-switch-when="1">回盘</span>
                    </td>
                    <td>{{item.projectId}}</td>
                </tr>
                </tbody>
            </table>
        </div>
        <div class="text-center">
            <button type="button" class="btn btn-default btn-primary" ng-click="leading()">
                确认导入
            </button>
        </div>
        </div>
    </div>
            <!--银行回盘结束-->
            <!--银行导入-->
    <div ng-show="show3">
        <div class="sel">
            <div class="row search-padding-10">
                <div class="col-lg-1 col-md-2 col-sm-2">
                    项目：
                </div>
                <div class="col-lg-3 col-md-4 col-sm-4">
                    <select class="form-control" ng-model="financial.projectId" ng-options="option.projectId as option.projectName for option in projects">
                        <option value="">请选择</option>
                    </select>
                </div>
                <div class="col-lg-1 col-md-2 col-sm-2 text-center">
                    银行：
                </div>
                <div class="col-lg-3 col-md-4 col-sm-4">
                    <select class="form-control" ng-model="financial.bankType">
                        <option value="">全部</option>
                        <option value="0100">邮政储蓄局</option>
                        <option value="0102">中国工商银行</option>
                        <option value="0103">中国农业银行</option>
                        <option value="0105">中国建设银行</option>
                        <option value="0308">招商银行</option>
                        <option value="0305">中国民生银行</option>
                    </select>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-1 col-md-2 col-sm-2">
                    时间：
                </div>
                <div class="col-lg-3 col-md-4 col-sm-4">
                    <input class="form-control" type="date" ng-model="financial.tobankDate">
                </div>
                <div class="col-lg-1 col-md-2 col-sm-2 text-center">
                    ----
                </div>
                <div class="col-lg-3 col-md-4 col-sm-4">
                    <input class="form-control" type="date" ng-model="financial.deductionsDate">
                </div>
                <button type="button" class="btn btn-default btn-primary"ng-click="searchPaginator._load();">
                    <span class="glyphicon glyphicon-search btn_icon"></span>
                    查询
                </button>
            </div>
        </div>
        <div class="row padding">
            <div class="col-lg-12">
                <div class="search-padding-10">
                    总结文件
                </div>
                <div class="search-padding-10">
                    <table>
                        <thead>
                        <tr>
                            <th>项目</th>
                            <th>代办银行</th>
                            <th>收付单位代码</th>
                            <th>币种</th>
                            <th>收付标志</th>
                            <th>收付代码</th>
                            <th>交易文件</th>
                            <th>总户数</th>
                            <th>总金额</th>
                            <th>已划款户数</th>
                            <th>已划款金额</th>
                            <th>送银行日期</th>
                            <th>应划款日期</th>
                            <th>划款日期</th>
                        </tr>
                        </thead>
                        <tbody>
                        <tr ng-repeat="item in searchPaginator.object.financialGroupTotals" ng-show="item.financialGroupTotalId!=null" ng-click="getDatils(item.financialGroupTotalId)" ng-class="{'this_info':btnIndex==item.financialGroupTotalId}">
                            <td>{{item.projectName}}</td>
                            <td>{{item.bankType}}</td>
                            <td>{{item.bankNum}}</td>
                            <td ng-switch on="item.coinType">
                                <span ng-switch-when="0">人民币</span>
                                <span ng-switch-when="1">港币</span>
                            </td>
                            <td ng-switch on="item.paymentState">
                                <span ng-switch-when="0">已收</span>
                                <span ng-switch-when="1">未收</span>
                            </td>
                            <td>{{item.collectionTotalNum}}</td>
                            <td>{{item.collectionTotalName}}</td>
                            <td>{{item.houseNumber}}</td>
                            <td>{{item.sum}}</td>
                            <td>{{item.deductionsHouseNumber}}</td>
                            <td>{{item.deductionsSum}}</td>
                            <td>{{item.tobankDate|date:'yyyy-MM-dd'}}</td>
                            <td>{{item.prepareDeductionsDate|date:'yyyy-MM-dd'}}</td>
                            <td>{{item.deductionsDate|date:'yyyy-MM-dd'}}</td>
                        </tr>
                        </tbody>
                    </table>
                </div>
                <div class="search-padding-10">
                    <div class="bill_paging">
                        <div class="pagination">
                            <li><a style="cursor:pointer;" ng-click="searchPaginator.go(1)">首页</a></li>
                            <li><a class="cursor_pointer" ng-show="searchPaginator.hasPrevious()" ng-click="searchPaginator.previous()">上一页</a></li>
                            <li><a class="cursor_pointer" ng-repeat="i in searchPaginator.pages"  ng-click="searchPaginator.go(i)">{{i}}</a></li>
                            <li><a class="cursor_pointer" ng-show="searchPaginator.hasNext()" ng-click="searchPaginator.next()">下一页</a></li>
                            <li><a style="cursor:pointer;" ng-click="searchPaginator.go(searchPaginator.page.totalPage)">尾页</a></li>
                        </div>
                    </div>
                </div>
                <div class="search-padding-10">
                    明细文件
                </div>
                <div class="search-padding-10">
                    <table>
                        <thead>
                        <tr>
                            <th>合同号</th>
                            <th>行别</th>
                            <th>开户行号</th>
                            <th>账户</th>
                            <th>金额</th>
                            <th>扣款标记</th>
                            <th>户主</th>
                            <th>留言</th>
                        </tr>
                        </thead>
                        <tbody>
                        <tr ng-repeat="inn in financiala">
                            <td>{{inn.contractNumber}}</td>
                            <td>{{inn.bankType}}</td>
                            <td>{{inn.bankNum}}</td>
                            <td>{{inn.bankAccount}}</td>
                            <td>{{inn.sum}}</td>
                            <td>{{inn.deductionsTag}}</td>
                            <td>{{inn.custName}}</td>
                            <td>{{inn.message}}</td>
                        </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
<!--模态框开始-->
<div class="modal fade" id="ddd" tabindex="-1" role="dialog"
     aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close"
                        data-dismiss="modal" aria-hidden="true">
                    &times;
                </button>
                <h4 class="modal-title">
                    上传Excel表
                </h4>
            </div>
            <div class="modal-body">
                <div class="padding">
                <div id="remove">
                    <div id="excelupload" class="zyupload"></div>
                </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">取消</button>
                <button type="button" class="btn btn-primary" data-dismiss="modal">确认</button>
            </div>
        </div>
    </div>
</div>
<!--模态框开始-->
</body>
</html>