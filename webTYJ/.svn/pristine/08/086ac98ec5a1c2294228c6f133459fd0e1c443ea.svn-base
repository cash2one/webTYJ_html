<div class="modal fade" id="pickUp" role="dialog" aria-label="personDatil" aria-hidden="true">
    <div class="modal-dialog" >
        <div class="modal-content">
            <div class="modal-header">
                <span>登记拾到物品</span>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="row padding">
                    <div class="col-lg-12">
                        <div class="row search-padding-10">
                            <div class="col-lg-2 col-md-2 col-sm-2">
                                物品名称
                                </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <input type="text" ng-model="losts.articleName"  class="form-control">
                                </div>
                            </div>
                        <div class="row search-padding-10">
                            <div class="col-lg-2 col-md-2 col-sm-2">
                                联系人
                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <input type="text" ng-model="personone.name"  class="form-control">
                            </div>
                            <div class="col-lg-2 col-md-2 col-sm-2">
                                <button type="button" class="btn bg-primary" data-dismiss="modal" data-toggle="modal" data-target="#myModal_zuping">
                                    <span class="glyphicon glyphicon-plus btn_icon"></span>
                                    添加</button>
                            </div>
                            </div>
                        <div class="row search-padding-10">
                            <div class="col-lg-2 col-md-2 col-sm-2">
                                联系电话
                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <input type="text" ng-model="personone.phoneNum"  class="form-control">
                            </div>
                        </div>
                        <div class="row search-padding-10">
                            <div class="col-lg-2 col-md-2 col-sm-2">
                                拾到时间
                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <input type="text" ng-model="losts.articleTimePicker" class="form-control">
                            </div>
                        </div>
                        <div class="row search-padding-10">
                            <div class="col-lg-12 col-md-2 col-sm-2">
                                物品描述
                            </div>
                            <div class="col-lg-12 col-md-4 col-sm-4">
                                <textarea  ng-model="losts.articleDescribe" class="form-control"></textarea>
                            </div>
                        </div>
                        <div class="row search-padding-10">
                            <div class="col-lg-12 col-md-2 col-sm-2">
                                拾到地点描述
                            </div>
                            <div class="col-lg-12 col-md-4 col-sm-4">
                                <textarea  ng-model="losts.pickPlace" class="form-control"></textarea>
                            </div>
                        </div>

                        </div>
                </div>
            </div><div class="modal-footer">
            <button type="button" class="btn btn-default" data-dismiss="modal" >取消</button>
            <button type="button" class="btn bg-primary" data-dismiss="modal" ng-click="insert3()">保存</button>
        </div>

        </div>
    </div>
</div>

