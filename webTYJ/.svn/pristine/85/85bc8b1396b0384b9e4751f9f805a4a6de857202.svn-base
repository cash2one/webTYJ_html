    <!--实现单选效果-->
    <script>
        function check(obj) {
            $(':checkbox').each(function () {
                if (this != obj)
                    $(this).attr("checked", false);
                else {
                    if ($(this).prop("checked")){
                        $(this).attr("checked", true);
                    }
                    else
                        $(this).attr("checked", false);
                }
            });
        }
    </script>

    <script type="text/javascript">
        function exportExcel(){
            var url = document.getElementById("fileUrl").value;
            var custType =document.getElementById("custType").value;
            var projectId =document.getElementById("projectId").value;
            var name = $("#name").val();
            var cardType = $("#cardType").val();
            var cardNum = $("#cardNum").val();
            /*window.location.href=url +"/info/outPersonCustExcel.do"*/
           window.location.href=url +"/info/outPersonCustExcel.do?cardType="+encodeURI(encodeURI(cardType))+"&custType="+encodeURI(encodeURI(custType))
            +"&projectId="+encodeURI(encodeURI(projectId)) +"&name="+encodeURI(encodeURI(name)) +"&cardNum="+encodeURI(encodeURI(cardNum))
        }
    </script>

    <script type="text/javascript">
        function importExcelTemplet(){
            var url = document.getElementById("fileUrls").value;
            window.location.href=url +"/info/importExcelTemplet.do"
        }
    </script>

