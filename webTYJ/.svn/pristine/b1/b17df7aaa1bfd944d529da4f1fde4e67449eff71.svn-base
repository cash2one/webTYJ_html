                    <div class="col-lg-4 col-md-4 col-sm-4">
                    </div>
                    <div class="col-lg-4 col-md-4 col-sm-4 text-right">
                        <button type="button" class="btn btn-default btn-primary" ng-click="importTxt()"data-toggle="modal" data-target="#ddd" data-toggle="tab">
                            导入回盘文件
                        </button>
                    </div>
                    <div class="clear"></div>
                </div>
                <div class="search-padding-10">
                    <table>
                        <thead>
                        <tr>
                            <th> 代办银行号</th>
                            <th>收付单位代码</th>
                            <th>币种</th>
                            <th>收付标志</th>
                            <th>收付代码</th>
                            <th>交易文件名</th>
                            <th>总户数</th>
                            <th>总金额</th>
                            <th>已划款户数</th>
                            <th>已划款金额</th>
                            <th>送银行日期</th>
                            <th>应划款日期</th>
                            <th>划款日期</th>
                            <th>批次号</th>
                        </tr>
                        </thead>
                        <tbody>

                        <tr ng-repeat="item in searchOfferPaginator.object.financialCounterofferTotals" ng-show="item.financialCounterofferTotalId!=null"
                            ng-click="getDatilsOffer(item.financialCounterofferTotalId)" ng-class="{'this_info':btnIndex==item.financialCounterofferTotalId}">
                            <td>{{item.agencyBankNum}}</td>
                            <td>{{item.paymentUnitCode}}</td>
                            <td ng-switch on="item.coinType">
                                <span ng-switch-when="0">人民币</span>
                                <span ng-switch-when="1">港币</span>
                            </td>
                            <td ng-switch on="item.paymentState">
                                <span ng-switch-when="0">已收</span>
                                <span ng-switch-when="1">未收</span>
                            </td>
                            <td>{{item.collectionTotalNum}}</td>
                            <td>{{item.collectionTotalName}}</td>
                            <td>{{item.houseNumber}}</td>
                            <td>{{item.sum}}</td>
                            <td>{{item.deductionsHouseNumber}}</td>
                            <td>{{item.deductionsSum}}</td>
                            <td>{{item.tobankDate|date:'yyyy-MM-dd'}}</td>
                            <td>{{item.prepareDeductionsDate|date:'yyyy-MM-dd'}}</td>
                            <td>{{item.deductionsDate|date:'yyyy-MM-dd'}}</td>
                            <td>{{item.batchNumber}}</td>
                        </tr>
                        <tr ng-show="searchOfferPaginator.object.financialCounterofferTotals.length==null" >
                            <td colspan="15">暂无数据</td>
                        </tr>
                        </tbody>
                    </table>
                </div>
                <div class="row search-padding-10">
                    <div class="bill_paging">
                        <div class="bill_paging">
                            <span>共{{searchOfferPaginator.object.page.totalResult}}条</span>
                            <ul class="pagination">
                                <li ng-class="{true:'disabled'}[searchOfferPaginator.page.currentPage==1]"><a href="javascript:void(0)" ng-click="searchOfferPaginator.go(1)">首页</a></li>
                                <li ng-class="{true:'disabled'}[searchOfferPaginator.page.currentPage==1]"><a href="javascript:void(0)" ng-show="searchOfferPaginator.hasPrevious()" ng-click="searchOfferPaginator.previous()">上一页</a></li>
                                <li ng-class="{true:'active'}[searchOfferPaginator.page.currentPage==i]" ng-repeat="i in searchOfferPaginator.pages" ><a href="javascript:void(0)" on-show="searchOfferPaginator.go(i)"  ng-click="searchOfferPaginator.go(i)">{{i}}</a></li>
                                <li ng-class="{true:'disabled'}[searchOfferPaginator.page.currentPage==searchOfferPaginator.page.totalPage]"><a href="javascript:void(0)" ng-show="searchOfferPaginator.hasNext()" ng-click="searchOfferPaginator.next()">下一页</a></li>
                                <li ng-class="{true:'disabled'}[searchOfferPaginator.page.currentPage==searchOfferPaginator.page.totalPage]"><a href="javascript:void(0)" ng-click="searchOfferPaginator.go(searchOfferPaginator.page.totalPage)">尾页</a></li>
                            </ul>
                            <div class="clear"></div>
                        </div>
                    </div>
                </div>

                <div class="search-padding-10">
                    明细文件
                </div>
                <div class="search-padding-10">
                    <table>
                        <thead>
                        <tr>
                            <th>合同号</th>
                            <th>行别</th>
                            <th>开户行号</th>
                            <th>银行账号</th>
                            <th>金额</th>
                            <th>扣款标记</th>
                            <th>户主</th>
                            <th>留言</th>
                            <th>批次号</th>
                        </tr>
                        </thead>
                        <tbody>
                        <tr  ng-repeat="item in searchOfferPaginatorDetail.object.financialCounterofferDetails"
                             ng-show="item.financialCounterofferDetailId!=null"   >
                            <td>{{item.contractNumber}}</td>
                            <td>{{item.bankType}}</td>
                            <td>{{item.bankNum}}</td>
                            <td>{{item.bankAccount}}</td>
                            <td>{{item.sum}}</td>
                            <td>{{item.deductionsTag}}</td>
                            <td>{{item.custName}}</td>
                            <td>{{item.message}}</td>
                            <td>{{item.batchNumber}}</td>
                        </tr>
                        <tr ng-if="searchOfferPaginatorDetail.object.financialCounterofferDetails.length==null"><td colspan="9">暂无数据</td></tr>
                        </tbody>
                    </table>
                    <div class="padding">
                        <div class="bill_paging">
                            <span>共{{searchOfferPaginatorDetail.object.page.totalResult}}条</span>
                            <ul class="pagination">
                                <li ng-class="{true:'disabled'}[searchOfferPaginatorDetail.page.currentPage==1]"><a href="javascript:void(0)" ng-click="searchOfferPaginatorDetail.go(1)">首页</a></li>
                                <li ng-class="{true:'disabled'}[searchOfferPaginatorDetail.page.currentPage==1]"><a href="javascript:void(0)" ng-show="searchOfferPaginatorDetail.hasPrevious()" ng-click="searchOfferPaginatorDetail.previous()">上一页</a></li>
                                <li ng-class="{true:'active'}[searchOfferPaginatorDetail.page.currentPage==i]" ng-repeat="i in searchOfferPaginatorDetail.pages" ><a href="javascript:void(0)" on-show="searchOfferPaginatorDetail.go(i)"  ng-click="searchOfferPaginatorDetail.go(i)">{{i}}</a></li>
                                <li ng-class="{true:'disabled'}[searchOfferPaginatorDetail.page.currentPage==searchOfferPaginatorDetail.page.totalPage]"><a href="javascript:void(0)" ng-show="searchOfferPaginatorDetail.hasNext()" ng-click="searchOfferPaginatorDetail.next()">下一页</a></li>
                                <li ng-class="{true:'disabled'}[searchOfferPaginatorDetail.page.currentPage==searchOfferPaginatorDetail.page.totalPage]"><a href="javascript:void(0)" ng-click="searchOfferPaginatorDetail.go(searchOfferPaginatorDetail.page.totalPage)">尾页</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!--模态框开始-->
<div class="modal fade" id="ddd" tabindex="-1" role="dialog"
     aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close"
                        data-dismiss="modal" aria-hidden="true">
                    &times;
                </button>
                <h4 class="modal-title">
                    导入金融联回盘文件
                </h4>
            </div>
            <div class="modal-body">
                <div class="padding">
                <div id="remove">
                    <div id="excelupload" class="zyupload"></div>
                </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">取消</button>
                <button type="button" class="btn btn-primary" data-dismiss="modal"  ng-click="confirmImport()">确认</button>
            </div>
        </div>
    </div>
</div>
<!--模态框开始-->
</body>
</html>