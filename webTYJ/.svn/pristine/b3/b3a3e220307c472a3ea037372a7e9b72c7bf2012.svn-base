<div class="modal fade" id="tree" tabindex="-1" role="dialog"
     aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close"
                        data-dismiss="modal" aria-hidden="true">
                    &times;
                </button>
                <span >
                    地址
                </span>
            </div>
            <div class="modal-body">
                <div class="padding">
                    <div class="col-md-4">
                        <ul id="treeDemo"  class="ztree"></ul>
                    </div>

                </div>
                <div class="clear"></div>

            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">取消
                    <button class="btn bg-primary" data-dismiss="modal"   ng-click="addWaterReading()">提交</button>

                </button>
            </div>
        </div><!-- /.modal-content -->
    </div><!-- /.modal -->
</div>
<!--任务接受-->
<div class="modal fade" id="accept" tabindex="-1" role="dialog"
     aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close"
                        data-dismiss="modal" aria-hidden="true">
                    &times;
                </button>
                <span>
                    接受
                </span>>
            </div>
            <div class="modal-body">
                <div class="sel1">
                    <div class="row search-padding-10">
                        <div class="col-lg-12 col-md-2 col-sm-2">
                            说明
                        </div>
                        <div class="col-lg-12 col-md-4 col-sm-4">
                            <textarea class="text_min"  placeholder="请输入内容" ng-model="aaa.remarks"></textarea>
                        </div>
                    </div>

                </div>

            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">取消</button>
                <button class="btn bg-primary" data-dismiss="modal"   ng-click="UpdateAccept()">提交</button>
            </div>
        </div><!-- /.modal-content -->
    </div><!-- /.modal -->
</div>
<!--任务失效-->
<div class="modal fade" id="closed" tabindex="-1" role="dialog"
     aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close"
                        data-dismiss="modal" aria-hidden="true">
                    &times;
                </button>
                <span>
                    失效
                </span>
            </div>
            <div class="modal-body">
                <div class="sel1">
                    <div class="row search-padding-10">
                        <div class="col-lg-12 col-md-2 col-sm-2">
                            说明
                        </div>
                        <div class="col-lg-12 col-md-4 col-sm-4">
                            <textarea class="text_min"  placeholder="请输入内容" ng-model="aaa.remarks"></textarea>
                        </div>
                    </div>

                </div>

            </div>
            <div class="modal-footer">

                <button type="button" class="btn btn-default" data-dismiss="modal">取消
                </button>
                <button class="btn bg-primary" data-dismiss="modal"   ng-click="updateClosed()">提交</button>
            </div>
