                            <tr ng-repeat="item in searchPaginator2.object.teamworkStaffs" ng-show="item.id != null" ng-click="getdata1(item)">
                                <td><input type="checkbox"  name="aa"  ng-model="x" ng-checked="master" value={{item.id}} id="{{item.id}}" ng-click="getdata1(item)"></td>
                                <td>{{item.staff.staffName}}</td>
                                <td>{{item.specialtyInfo.specialtyName}}</td>
                                <td>{{item.staff.telphone}}</td>
                                <td>{{}}</td>
                            </tr>
                            </tbody>
                        </table>
                        <div class="padding">
                            <div class="bill_paging">
                                <span>共{{searchPaginator2.object.page.totalResult}}条</span>
                                <ul class="pagination">
                                    <li ng-class="{true:'disabled'}[searchPaginator2.page.currentPage==1]">
                                        <a href="javascript:void(0)" ng-click="searchPaginator2.go(1)">首页</a>
                                    </li>
                                    <li ng-class="{true:'disabled'}[searchPaginator2.page.currentPage==1]">
                                        <a href="javascript:void(0)" ng-show="searchPaginator2.hasPrevious()"
                                           ng-click="searchPaginator2.previous()">上一页</a>
                                    </li>
                                    <li ng-class="{true:'active'}[searchPaginator2.page.currentPage==i]"
                                        ng-repeat="i in searchPaginator2.pages">
                                        <a href="javascript:void(0)" ng-click="searchPaginator2.go(i)">{{i}}</a>
                                    </li>
                                    <li ng-class="{true:'disabled'}[searchPaginator2.page.currentPage==searchPaginator2.page.totalPage]">
                                        <a href="javascript:void(0)" ng-show="searchPaginator2.hasNext()" ng-click="searchPaginator2.next()">下一页</a>
                                    </li>
                                    <li ng-class="{true:'disabled'}[searchPaginator2.page.currentPage==searchPaginator2.page.totalPage]">
                                        <a href="javascript:void(0)" ng-click="searchPaginator2.go(searchPaginator2.page.totalPage)">尾页</a>
                                    </li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal" >取消</button>
                <button type="button" class="btn bg-primary" data-dismiss="modal" ng-click="addPersons1()">保存</button>
            </div>
        </div>
    </div>
</div>
<!--负责人员 END-->
</body>
</html>