                            <div class="col-lg-3 col-md-3 col-sm-3" ng-repeat="items in item.posts" ng-show="(items.postId !='19')&&(items.isCorePost!=1)">
                                <label class="checkbox inline">
                                    <input type="checkbox" class="checkboxa" name="checkbox" id={{items.postId}} value={{items.nameOfCustom}} ng-show="items.postId!=null">
                                    <span>{{items.nameOfCustom}}</span>
                                </label>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal" ng-click="close()">关闭</button>
                <button type="button" class="btn btn-primary"  data-dismiss="modal" aria-label="Close" ng-click="temp()">确认</button>
            </div>
        </div>
    </div>
