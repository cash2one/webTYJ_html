                </thead>
                <tbody>
                <tr ng-repeat="item in searchPaginator.object.pendingApprovals" ng-show="item.personVip.vipId!=null" ng-click="getdata(item,'aaa',$event)" ng-dblclick="showP(item)">
                    <td><input type="checkbox" id="{{item.personVip.vipId}}" name="aaa" ng-click="getdata(item,'aaa',$event)"></td>
                    <td>{{item.personVip.theme}}</td>
                    <td ng-switch on="item.personVip.applystype">
                        <span ng-switch-when="0">vip申请</span>
                        <span ng-switch-when="1">vip失效申请</span>
                        <span ng-switch-when="2">重点客户申请</span>
                        <span ng-switch-when="3">重点客户失效申请</span>
                    </td>
                    <td>{{item.personVip.name}}</td>
                    <td>{{item.personVip.vipStart|date:'yyyy-MM-dd'}}</td>

                    <td  ng-switch on="item.personVip.reviewState">
                        <span ng-switch-when="0">待审核</span>
                        <span ng-switch-when="1">完成</span>
                        <span ng-switch-when="2">驳回</span>
                    </td>
                </tr>
                <tr ng-repeat="items in searchPaginator.object.pendingApprovals" ng-show="items.personEmphasis.emphasisId!=null" ng-click="getdata(items,'aaa',$event)" ng-dblclick="showP(items)">
                    <td><input type="checkbox" id="{{items.personEmphasis.emphasisId}}"  name="aaa" ng-click="getdata(items，'aaa',$event)"></td>
                    <td>{{items.personEmphasis.theme1}}</td>
                    <td ng-switch on="items.personEmphasis.applystype1">
                        <span ng-switch-when="0">vip申请</span>
                        <span ng-switch-when="1">vip失效申请</span>
                        <span ng-switch-when="2">重点客户申请</span>
                        <span ng-switch-when="3">重点客户失效申请</span>
                    </td>
                    <td>{{items.personEmphasis.username}}</td>

                    <td>{{items.personEmphasis.applydate|date:'yyyy-MM-dd'}}</td>

                    <td ng-switch on="items.personEmphasis.applystatus">
                        <span ng-switch-when="0">待审核</span>
                        <span ng-switch-when="1">完成</span>
                        <span ng-switch-when="2">驳回</span>
                    </td>
                </tr>

                </tbody>
            </table>
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="padding">
                    <div class="bill_paging" ng-show="searchPaginator.object.page.totalResult">
                        <span>共{{searchPaginator.object.page.totalResult}}条</span>
                        <ul class="pagination">
                            <li ng-class="{true:'disabled'}[searchPaginator.page.currentPage==1]">
                                <a href="javascript:void(0)" ng-click="searchPaginator.go(1)">首页</a>
                            </li>
                            <li ng-class="{true:'disabled'}[searchPaginator.page.currentPage==1]">
                                <a href="javascript:void(0)" ng-show="searchPaginator.hasPrevious()"
                                   ng-click="searchPaginator.previous()">上一页</a>
                            </li>
                            <li ng-class="{true:'active'}[searchPaginator.page.currentPage==i]"
                                ng-repeat="i in searchPaginator.pages">
                                <a href="javascript:void(0)" ng-click="searchPaginator.go(i)">{{i}}</a>
                            </li>
                            <li ng-class="{true:'disabled'}[searchPaginator.page.currentPage==searchPaginator.page.totalPage]">
                                <a href="javascript:void(0)" ng-show="searchPaginator.hasNext()"
                                   ng-click="searchPaginator.next()">下一页</a>
                            </li>
                            <li ng-class="{true:'disabled'}[searchPaginator.page.currentPage==searchPaginator.page.totalPage]">
                                <a href="javascript:void(0)"
                                   ng-click="searchPaginator.go(searchPaginator.page.totalPage)">尾页</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
