<!DOCTYPE html>
<html>
<head lang="en">
    <meta charset="UTF-8">
    <title>售后管理</title>
</head>
<body>
<div id="content-first" ng-show="mainPage==0">
    <!--tab页切换开始-->
    <div id="tab">
        <a  ng-click="toPage(1,3)"  ng-class="{'this_action ':btnIndex==1}">售后管理</a>
        <a  lang="no" ng-click="toPage(2,0)" ng-class="{'this_action ':btnIndex==2}">业主函件管理</a>
    </div>

    <!--视图加载开始-->

    <div class="sel" id="show_after_sale_project" ng-show="btnIndex==1">
        <div class="row search-padding-10 text-left">
            <div class="col-lg-2 col-md-1 col-sm-2 ">售后时间设置：</div>
            <div class="col-lg-1 col-md-4 col-sm-4">
                <select  ng-model="setting.days"  class="form-control">
                    <option ng-true-value="''" value="''" selected>请选择</option>
                    <option  ng-repeat="index in days " value="{{index}}">{{index}}天</option>
                </select>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-12" ng-show="btnIndex==1">
                <table>
                    <thead>
                    <tr>
                        <th>序号</th>
                        <th>项目名称</th>
                        <th>项目售后开始日期</th>
                        <th>项目售后结束日期</th>
                        <th>所属公司</th>
                        <th>项目经理</th>
                    </tr>
                    </thead>
                    <tbody>
                    <tr ng-repeat="dep in loadProjectPaginator.object['project'] " ng-show="dep.projectId != null">
                        <td>{{$index}}</td>
                        <td ng-click="clickProjectName(dep)">{{dep.projectName}}</td>
                        <td>{{dep.afterSaleInfo.afterStartTime|date:'yyyy-MM-dd'}}</td>
                        <td>{{dep.afterSaleInfo.afterEndTime|date:'yyyy-MM-dd'}}</td>
                        <td>{{dep.companyDetail.companyName}}</td>
                        <td>{{dep.staff.staffName}}</td>
                    </tr>
                    </tbody>
                </table>
                <div class="bill_paging">
                    <div class="pagination">
                        <ul class="pagination">
                            <li ng-class="{true:'disabled'}[loadProjectPaginator.page.currentPage==1]">
                                <a href="javascript:void(0)" ng-click="loadProjectPaginator.go(1)">首页</a>
                            </li>
                            <li ng-class="{true:'disabled'}[loadProjectPaginator.page.currentPage==1]">
                                <a href="javascript:void(0)" ng-show="loadProjectPaginator.hasPrevious()" ng-click="loadProjectPaginator.previous()">上一页</a>
                            </li>
                            <li ng-class="{true:'active'}[loadProjectPaginator.page.currentPage==i]" ng-repeat="i in loadProjectPaginator.pages">
                                <a href="javascript:void(0)"  ng-click="loadProjectPaginator.go(i)">{{i}}</a>
                            </li>
                            <li ng-class="{true:'disabled'}[loadProjectPaginator.page.currentPage==loadProjectPaginator.page.totalPage]">
                                <a href="javascript:void(0)" ng-show="loadProjectPaginator.hasNext()" ng-click="loadProjectPaginator.next()">下一页</a>
                            </li>
                            <li ng-class="{true:'disabled'}[loadProjectPaginator.page.currentPage==loadProjectPaginator.page.totalPage]">
                                <a href="javascript:void(0)" ng-click="loadProjectPaginator.go(loadProjectPaginator.page.totalPage)">尾页</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="sel" id="show_after_sale_letters" ng-show="btnIndex==2&&show_after_sale_letters==1">
        <div class="row">
            <div class="col-lg-12" ng-show="btnIndex==2">
                <table>
                    <thead>
                    <tr>
                        <th>序号</th>
                        <th>项目名称</th>
                        <th>项目售后开始日期</th>
                        <th>项目售后结束日期</th>
                        <th>所属公司</th>
                        <th>项目经理</th>
                        <th>状态</th>
                    </tr>
                    </thead>
                    <tbody>
                    <tr ng-repeat="dep in loadProjectPaginator.object['project'] " ng-show="dep.projectId != null">
                        <td>{{$index}}</td>
                        <td>{{dep.projectName}}</td>
                        <td>{{dep.afterSaleInfo.afterStartTime|date:'yyyy-MM-dd'}}</td>
                        <td>{{dep.afterSaleInfo.afterEndTime|date:'yyyy-MM-dd'}}</td>
                        <td>{{dep.companyDetail.companyName}}</td>
                        <td>{{dep.staff.staffName}}</td>
                        <td ng-click="clickState()">{{dep.afterSaleInfo.process}}</td>
                    </tr>
                    </tbody>
                </table>
                <div class="bill_paging">
                    <div class="pagination">
                        <ul class="pagination">
                            <li ng-class="{true:'disabled'}[loadProjectPaginator.page.currentPage==1]">
                                <a href="javascript:void(0)" ng-click="loadProjectPaginator.go(1)">首页</a>
                            </li>
                            <li ng-class="{true:'disabled'}[loadProjectPaginator.page.currentPage==1]">
                                <a href="javascript:void(0)" ng-show="loadProjectPaginator.hasPrevious()" ng-click="loadProjectPaginator.previous()">上一页</a>
                            </li>
                            <li ng-class="{true:'active'}[loadProjectPaginator.page.currentPage==i]" ng-repeat="i in loadProjectPaginator.pages">
                                <a href="javascript:void(0)"  ng-click="loadProjectPaginator.go(i)">{{i}}</a>
                            </li>
                            <li ng-class="{true:'disabled'}[loadProjectPaginator.page.currentPage==loadProjectPaginator.page.totalPage]">
                                <a href="javascript:void(0)" ng-show="loadProjectPaginator.hasNext()" ng-click="loadProjectPaginator.next()">下一页</a>
                            </li>
                            <li ng-class="{true:'disabled'}[loadProjectPaginator.page.currentPage==loadProjectPaginator.page.totalPage]">
                                <a href="javascript:void(0)" ng-click="loadProjectPaginator.go(loadProjectPaginator.page.totalPage)">尾页</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="sel" id="" ng-show="btnIndex==2&&show_after_sale_letters==2">
        <div class="row">
            <div class="col-lg-12" ng-show="btnIndex==2">
                <table>
                    <thead>
                    <tr>
                        <th>序号</th>
                        <th>指定发函日期</th>
                        <th>业主函件</th>
                        <th>录快递单号</th>
                        <th>状态</th>
                    </tr>
                    </thead>
                    <tbody>
                    <tr ng-repeat="dep in loadProjectPaginator.object['project'] " ng-show="dep.projectId != null">
                        <td>{{$index}}</td>
                        <td>2014-1-3</td>
                        <td>打印</td>
                        <td ng-click="loadOne()">录入</td>
                        <td>未发函</td>
                    </tr>
                    </tbody>
                </table>
                <div class="bill_paging">
                    <div class="pagination">
                        <ul class="pagination">
                            <li ng-class="{true:'disabled'}[Psearchaginator.page.currentPage==1]">
                                <a href="javascript:void(0)" ng-click="Psearchaginator.go(1)">首页</a>
                            </li>
                            <li ng-class="{true:'disabled'}[Psearchaginator.page.currentPage==1]">
                                <a href="javascript:void(0)" ng-show="Psearchaginator.hasPrevious()" ng-click="Psearchaginator.previous()">上一页</a>
                            </li>
                            <li ng-class="{true:'active'}[Psearchaginator.page.currentPage==i]" ng-repeat="i in Psearchaginator.pages">
                                <a href="javascript:void(0)"  ng-click="Psearchaginator.go(i)">{{i}}</a>
                            </li>
                            <li ng-class="{true:'disabled'}[Psearchaginator.page.currentPage==searchFaq.page.totalPage]">
                                <a href="javascript:void(0)" ng-show="Psearchaginator.hasNext()" ng-click="Psearchaginator.next()">下一页</a>
                            </li>
                            <li ng-class="{true:'disabled'}[Psearchaginator.page.currentPage==Psearchaginator.page.totalPage]">
                                <a href="javascript:void(0)" ng-click="Psearchaginator.go(Psearchaginator.page.totalPage)">尾页</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
        <div class="row text-center">
            <button type="button" class="btn btn-primary" ng-click="backToHome2()">
                <span class="glyphicon glyphicon-backward"></span>
                返回
            </button>
        </div>
    </div>

    <div class="sel" id="after_sale_in_project" ng-show="btnIndex==3">
        <div class="row search-padding-10">
            <div class="col-lg-1 col-md-2 col-sm-2 text-right">项目：</div>
            <div class="col-lg-3 col-md-4 col-sm-4">
                <select name="select4" ng-model="deplist"  class="form-control" id="select4">
                    <option ng-true-value="''" value="''" selected>--请选择--</option>
                    <option  ng-repeat="dep in Project" value="{{dep.projectId}}" ng-click="showlist()">{{dep.projectName}}</option>
                </select>
            </div>
            <div class="col-lg-1 col-md-2 col-sm-2 text-right">资产名称：</div>
            <div class="col-lg-3 col-md-4 col-sm-4">
                <form><input name="input" type="text" class="form-control" placeholder="请输入搜索条件" ng-model="pptname" style="margin-right: 10px;"></form>
            </div>
            <div class="col-lg-4 col-md-4 col-sm-4 text-left">
                <button type="button" class="btn btn-default btn-primary" ng-click="searchInfo()">
                    <span class="glyphicon glyphicon-search btn_icon"></span>
                    查询
                </button>
            </div>
            <div class="col-lg-1 col-md-2 col-sm-2 text-right">来源：</div>
            <div class="col-lg-1 col-md-2 col-sm-2">
                <label class="checkbox inline">
                    <input name="" type="checkbox" ng-true-value="'验房'" ng-false-value="''" ng-model="AfterSaleQuery.source" ng-click="show()">
                    <span>验房</span>
                </label>
            </div>
            <div class="col-lg-1 col-md-2 col-sm-2">
                <label class="checkbox inline">
                    <input name="" type="checkbox" ng-true-value="'入伙'" ng-false-value="''" ng-model="AfterSaleQuery.source" ng-click="show()">
                    <span>入伙</span>
                </label>
            </div>
            <div class="col-lg-1 col-md-2 col-sm-2">
                <label class="checkbox inline">
                    <input name="" type="checkbox" ng-true-value="'维修'" ng-false-value="''" ng-model="AfterSaleQuery.source">
                    <span>维修</span>
                </label>
            </div>
            <div class="col-lg-1 col-md-2 col-sm-2 text-right">进度：</div>
            <div class="col-lg-1 col-md-2 col-sm-2">
                <label class="checkbox inline">
                    <input name="" type="checkbox" ng-true-value="'处理完'" ng-false-value="''" ng-model="AfterSaleQuery.process" ng-click="show()">
                    <span>处理完</span>
                </label>
            </div>
            <div class="col-lg-1 col-md-2 col-sm-2">
                <label class="checkbox inline">
                    <input name="" type="checkbox" ng-true-value="'已受理'" ng-false-value="''" ng-model="AfterSaleQuery.process" ng-click="show()">
                    <span>已受理</span>
                </label>
            </div>
            <div class="col-lg-1 col-md-2 col-sm-2">
                <label class="checkbox inline">
                    <input name="" type="checkbox" ng-true-value="'处理中'" ng-false-value="''" ng-model="AfterSaleQuery.process">
                    <span>维修</span>
                </label>
            </div>
        </div>
        <div class="row search-padding-10" style="margin-top:-10px">
            <div class="col-lg-1 col-md-2 col-sm-2 text-right">专业：</div>
            <div class="col-lg-1 col-md-2 col-sm-2 text-left"ng-repeat="pro in PropertyType">
                <!--<div class="col-lg-1 col-md-2 col-sm-2 text-right" ng-show="$index!=0 && ($index%5) == 0"></div>-->
                <label class="checkbox inline">
                    <input  name="propertypename" type="checkbox" id="{{pro.propertyTypeName}}">
                    <span>{{pro.propertyTypeName}}</span>
                </label>
            </div>
        </div>
    </div>
    <div class="row padding">
        <div class="col-lg-12" id="show_after_sale_list" ng-show="btnIndex==3 && show_search_after_sale==0">
            <div class="row text-right">
                <button type="button" class="btn btn-default btn-primary" ng-click="clickAfterSaleCompany()">
                    发函售后责任单位
                </button>
                <button type="button" class="btn btn-default btn-primary" ng-click="clickRepairCompany()">
                    发函维修委托单位
                </button>
            </div>
            <div class="sel" ng-show="btnIndex==3 && btn_after_sale==0">
                <div class="row">
                    <table>
                        <thead>
                        <tr>
                            <th width="50px"/>
                            <th>序号</th>
                            <th>地址</th>
                            <th>专业</th>
                            <th>区域</th>
                            <th>资产编号</th>
                            <th>资产名称</th>
                            <th>业主</th>
                            <th>业主电话</th>
                            <th>处理状态</th>
                            <th>来源</th>
                            <th>问题描述</th>
                            <th>保修单位</th>
                            <th>维修验收</th>
                        </tr>
                        </thead>
                        <tbody>
                        <tr ng-repeat="item in Psearchaginator.object.afterSales" ng-show="item.id != null" ng-click="clickAfterSaleList()">
                            <td><input type="checkbox" id="item.id" name="item.name"  ng-checked=""></td>
                            <td>{{$index}}</td>
                            <td>{{item.homeAddress}}</td>
                            <td>{{item.propertyTypeName}}</td>
                            <td>{{item.area}}</td>
                            <td>{{item.propertyNumber}}</td>
                            <td>{{item.propertyName}}</td>
                            <td>{{item.homeAddress}}</td>
                            <td>{{item.clientPhone}}</td>
                            <td>{{item.process}}</td>
                            <td>{{item.source}}</td>
                            <td>{{item.describe}}</td>
                            <td>
                                <div ng-switch="item.guarantee">
                                    <span ng-switch-when="0">已接受保修</span>
                                    <span ng-switch-when="1">不保修，发送请求</span>
                                    <span ng-switch-when="2">已发送请求</span>
                                    <span ng-switch-default>接受保修&nbsp;/&nbsp;<a href="#" ng-click="sendnotice(item);">发送请求</a></span>
                                </div>
                            </td>
                            <td></td>
                        </tr>
                        </tbody>
                    </table>
                    <div class="bill_paging">
                        <div class="pagination">
                            <ul class="pagination">
                                <li ng-class="{true:'disabled'}[Psearchaginator.page.currentPage==1]">
                                    <a href="javascript:void(0)" ng-click="Psearchaginator.go(1)">首页</a>
                                </li>
                                <li ng-class="{true:'disabled'}[Psearchaginator.page.currentPage==1]">
                                    <a href="javascript:void(0)" ng-show="Psearchaginator.hasPrevious()" ng-click="Psearchaginator.previous()">上一页</a>
                                </li>
                                <li ng-class="{true:'active'}[Psearchaginator.page.currentPage==i]" ng-repeat="i in Psearchaginator.pages">
                                    <a href="javascript:void(0)"  ng-click="Psearchaginator.go(i)">{{i}}</a>
                                </li>
                                <li ng-class="{true:'disabled'}[Psearchaginator.page.currentPage==Psearchaginator.page.totalPage]">
                                    <a href="javascript:void(0)" ng-show="Psearchaginator.hasNext()" ng-click="Psearchaginator.next()">下一页</a>
                                </li>
                                <li ng-class="{true:'disabled'}[Psearchaginator.page.currentPage==Psearchaginator.page.totalPage]">
                                    <a href="javascript:void(0)" ng-click="Psearchaginator.go(Psearchaginator.page.totalPage)">尾页</a>
