<!--
    页面名称：【专业资产初始化】新增资产
    前端负责人：NM
    BUG 641 朱琪 2016.02.23
-->
<!DOCTYPE html>
<html>
<head lang="en">
    <meta charset="UTF-8">
    <title>物业工作台</title>
    <link rel="stylesheet" href="bower_components/skins/zyupload-1.0.0.min.css " type="text/css">
    <script type="text/javascript">
        //导出模板 朱琪 2016.02.23
        function importExcelTemplet(){
            var url = document.getElementById("fileUrl").value;
            window.location.href=url +"/info/importPropertyTemplet.do"
        }


    </script>
</head>
<body>
<div class="row padding">
<div class="row search-padding-10" >
    <div class="col-lg-1 col-md-2 col-sm-2  text-right">
        专业名称:
    </div>
    <div class="col-lg-3 col-md-4 col-sm-4">
            <input type="text" class="form-control" id="propertytypename" ng-model="PropertyTypeResult.property.propertytypename"  placeholder="搜索资产相关内容">
    </div>
    <div class="col-lg-1 col-md-2 col-sm-2  text-right">
        分类名称:
    </div>
    <div class="col-lg-3 col-md-4 col-sm-4">

            <input type="text" class="form-control" id="propertytypenames" ng-model="PropertyTypeResult.property.propertytypenames"  placeholder="搜索资产相关内容">

    </div>
    <div class="col-lg-1 col-md-2 col-sm-2  text-right">
        资产名称:
    </div>
    <div class="col-lg-3 col-md-4 col-sm-4">
        <form id="form2" name="form1" method="post" action="">
            <input type="text" class="form-control" id="propertyName"  ng-model="PropertyTypeResult.property.propertyName"  placeholder="搜索资产相关内容">
        </form>
    </div>

</div>
    <div class="row search-padding-10" >
        <div class="col-lg-1 col-md-2 col-sm-2  text-right">
            产地:
        </div>
        <div class="col-lg-3 col-md-4 col-sm-4">
            <form id="form3" name="form1" method="post" action="">
                <input type="text" class="form-control" id="producingArea"  ng-model="PropertyTypeResult.property.producingArea"  placeholder="搜索资产相关内容">
            </form>
        </div>
        <div class="col-lg-1 col-md-2 col-sm-2  text-right">
            品牌:
        </div>
        <div class="col-lg-3 col-md-4 col-sm-4">
            <form id="form4" name="form1" method="post" action="">
                <input type="text" class="form-control" id="brand"  ng-model="PropertyTypeResult.property.brand" placeholder="搜索资产相关内容">
            </form>
        </div>
        <div class="col-lg-1 col-md-2 col-sm-2  text-right">
            厂商:
        </div>
        <div class="col-lg-3 col-md-4 col-sm-4">
            <form id="form5" name="form1" method="post" action="">
                <input type="text" class="form-control" id="manufacturer" ng-model="PropertyTypeResult.property.manufacturer" placeholder="搜索资产相关内容">
            </form>
        </div>

    </div>
    <div class="row search-padding-10" >
    <div class="col-lg-12 col-md-2 col-sm-2  text-right">
        <!--禅道 BUG 176-->
        <button type="button" class="btn btn-default btn-primary" ng-click="listPropertys._load()">
            <span class="glyphicon glyphicon-search btn_icon"></span>搜索</button>
    </div>
        </div>

<div class="building_padding">
    <input type="hidden" id="fileUrl" value="{{fileUrl}}">
    <input type="hidden" id="property" value="{{PropertyTypeResult.property}}">
    <button type="button" data-toggle="modal" class="btn btn-default btn-primary" onclick="importExcelTemplet()">模板下载</button>
    <!--Bug826 陈浪 2016.3.4-->
    <button role="presentation" data-toggle="modal" data-target="#ddd" class="btn btn-default btn-primary" tabindex="-1" data-toggle="tab" ng-click="inportExcel()">导入</button>
    <button type="button" data-toggle="modal" class="btn btn-default btn-primary" onclick="exportPropertyExcel()">导出</button>
    <!--Bug837 陈浪  2016年3月3日-->
    <button type="button" class="btn btn-default btn-primary" ng-click="doClick(2)" ui-sref="index.baseManagement.majorAssetInitialization.newAssets">
        <span class="glyphicon glyphicon-plus btn_icon"></span>
        新增资产
    </button>
    <button type="button" class="btn btn-default btn-primary" disabled="disabled" ng-disagbled="true">
        <span class="glyphicon glyphicon-remove btn_icon"></span>
        删除
    </button>
</div>
    <div class="search-padding-10">
            <!-- 将树形修改成表格 朱琪2016.02.22 starta -->
            <table>
                <thead>
                <tr>
                    <th>专业编号</th>
                    <th>专业名称</th>
                    <th>专业分类编号</th>
                    <th>专业分类名称</th>
                    <th>资产编号</th>
                    <th>资产名称</th>
                    <th>规格型号</th>
                    <th>品牌</th>
                    <th>产地</th>
                    <th>厂商</th>
                    <th>厂商电话</th>
                </tr>
                </thead>
                <tbody>
                <!--Bug830 陈浪 2016年3月4号-->
                <tr  ng-repeat="property in listPropertys.object.propertys" ng-show="property.propertyId != null">
                    <td>{{property.propertytype.propertyType.propertyTypeNumber}}</td>
                    <td>{{property.propertytype.propertyType.propertyTypeName}}</td>
                    <td>{{property.propertytype.propertyTypeNumber}}</td>
                    <td>{{property.propertytype.propertyTypeName}}</td>
                    <td>{{property.propertytypeNumber}}</td>
                    <td>{{property.propertyName}}</td>
                    <td>{{property.propertySpecification}}</td>
                    <td>{{property.brand}}</td>
                    <td>{{property.producingArea}}</td>
                    <td>{{property.manufacturer}}</td>
                    <td>{{property.manufacturerPhone}}</td>
                </tr>
                <tr ng-if="listPropertys.object.page.totalResult == 0">
                    <td colspan="8">没有数据！</td>
                </tr>
                </tbody>
            </table>
            <!-- 将树形修改成表格 朱琪2016.02.22 end -->
            <div class="padding">
                <div class="bill_paging">
                    <span>共{{listPropertys.object.page.totalResult}}条</span>
                    <ul class="pagination">
                        <li ng-class="{true:'disabled'}[listPropertys.page.currentPage==1]">
                            <a href="javascript:void(0)" ng-click="listPropertys.go(1)">首页</a>
                        </li>
                        <li ng-class="{true:'disabled'}[listPropertys.page.currentPage==1]">
                            <a href="javascript:void(0)" ng-show="listPropertys.hasPrevious()" ng-click="listPropertys.previous()">上一页</a>
                        </li>
                        <li ng-class="{true:'active'}[listPropertys.page.currentPage==i]" ng-repeat="i in listPropertys.pages">
                            <a href="javascript:void(0)"  ng-click="listPropertys.go(i)">{{i}}</a>
                        </li>
                        <li ng-class="{true:'disabled'}[listPropertys.page.currentPage==listPropertys.page.totalPage]">
                            <a href="javascript:void(0)" ng-show="listPropertys.hasNext()" ng-click="listPropertys.next()">下一页</a>
                        </li>
                        <li ng-class="{true:'disabled'}[listPropertys.page.currentPage==listPropertys.page.totalPage]">
                            <a href="javascript:void(0)" ng-click="listPropertys.go(listPropertys.page.totalPage)">尾页</a>
                        </li>
                    </ul>
                </div>
            </div>
       </div>
    </div>
<div class="modal fade" id="ddd" tabindex="-1" role="dialog"
     aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog" >
        <div class="modal-content">
            <div class="modal-header">
                上传Excel表
                <button type="button" class="close"
                        data-dismiss="modal" aria-hidden="true">
                    &times;
                </button>
            </div>
            <div class="modal-body">
                <div class="padding"  id="remove1">
                    <div id="excelupload" class="zyupload"></div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" ng-click="cancleExcel()">取消 </button>
                <button class="btn btn-default btn-primary" data-dismiss="modal"  >确认</button>
            </div>
        </div>
    </div>
</div>

