<!--负责人员-->
<div class="modal fade" id="newPerson" role="dialog" aria-label="personDatil" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <span>人员</span>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="padding">
                    <div class="sel1">
                        <div class="row search-padding-10">
                            <div class="col-lg-2 col-md-2 col-sm-2">
                                姓名
                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <input class="form-control" ng-model="Teamworkstaff.staffName"/>
                            </div>
                            <div class="col-lg-2 col-md-2 col-sm-2">
                                电话
                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <input class="form-control" ng-model="Teamworkstaff.telphone"/>
                            </div>
                            <div class="col-lg-2 col-md-2 col-sm-2">
                                部门
                            </div>
                            <div class="col-lg-4 col-md-4 col-sm-4">
                                <select class="form-control" ng-model="Teamworkstaff.professionalLineId"
                                        ng-options="a.specialtyId as a.specialtyName for a in SpecialtyInfo">
                                    <option value="">选择部门</option>
                                </select>
                            </div>
                            <div class="col-lg-6 col-md-6 col-sm-6">
                                <button type="button" class="btn btn-default btn-primary"
                                        ng-click="searchPaginator2._load()">
                                    <span class="glyphicon glyphicon-search btn_icon"></span>搜索
                                </button>
                            </div>
                        </div>
                    </div>
                    <div class="search-padding-10">
                        <table>
                            <thead>
                            <th></th>
                            <th>姓名</th>
                            <th>部门</th>
                            <th>电话</th>
                            <th>状态</th>
                            </thead>
                            <tbody>
                            <tr ng-repeat="item in searchPaginator2.object.teamworkStaffs" ng-show="item.id != null">
                                <td><input type="checkbox" name="aaa" ng-click="getdata(item)"></td>
                                <td>{{item.staff.staffName}}</td>
                                <td>{{item.specialtyInfo.specialtyName}}</td>
                                <td>{{item.staff.telphone}}</td>
                                <td>{{}}</td>
                            </tr>
                            </tbody>
                        </table>
                        <div class="bill_paging">
                            <ul class="pagination">
                                <li><a style="cursor:pointer;" ng-click="searchPaginator2.go(1)">首页</a></li>
                                <li><a style="cursor:pointer;" ng-show="searchPaginator2.hasPrevious()"
                                       ng-click="searchPaginator2.previous()">上一页</a></li>
                                <li><a style="cursor:pointer;" ng-repeat="i in searchPaginator2.pages"
                                       ng-click="searchPaginator2.go(i)">{{i}}</a></li>
                                <li><a style="cursor:pointer;" ng-show="searchPaginator2.hasNext()"
                                       ng-click="searchPaginator2.next()">下一页</a></li>
                                <li><a style="cursor:pointer;"
                                       ng-click="searchPaginator2.go(searchPaginator2.page.totalPage)">尾页</a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">取消</button>
                <button type="button" class="btn bg-primary" data-dismiss="modal" ng-click="addPerson()">保存</button>
            </div>
        </div>
    </div>
</div>
<!--负责人员 END-->
</div>
</div>
</body>
</html>