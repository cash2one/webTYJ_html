                <button class="btn bg-primary" data-dismiss="modal" ng-click="updateSpecial()">提交</button>
            </div>
        </div><!-- /.modal-content -->
    </div><!-- /.modal -->
</div>
<!--任务集中处理-->
<div class="modal fade" id="concentrated" tabindex="-1" role="dialog"
     aria-labelledby="myModalLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close"
                        data-dismiss="modal" aria-hidden="true">
                    &times;
                </button>
                <span>
                    集中处理
                </span>
            </div>
            <div class="modal-body">
                <div class="padding">
                    <div class="row search-padding-10">
                        <div class="col-lg-3 col-md-2 col-sm-2">
                            选择转入集中处理项
                        </div>
                        <div class="col-lg-4 col-md-2 col-sm-2">
                            <select class="form-control" ng-model="aaa.projectId"
                                    ng-options="temp.projectId as temp.projectName for temp in centralizedTreatmentProjects">
                                <option value="">请选择集中处理类型</option>
                            </select>
                        </div>
                    </div>

                    <div class="row search-padding-10">
                        <div class="col-lg-12 col-md-2 col-sm-2">
                            说明
                        </div>
                        <div class="col-lg-12 col-md-4 col-sm-4">
                            <textarea class="text_min" cols="3" placeholder="请输入内容" ng-model="aaa.remarks"></textarea>
                        </div>
                    </div>


                </div>

            </div>
            <div class="modal-footer">

                <button type="button" class="btn btn-default" data-dismiss="modal">取消
