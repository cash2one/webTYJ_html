<!DOCTYPE html>
<html>
<head lang="en">
    <meta charset="UTF-8">
    <title>计费方案</title>
</head>
<body>
<div>
    <div class="sel">
        <div class="row search-padding-10">
            <div class="col-lg-1 col-md-2 col-sm-2">
                物业项目
            </div>
            <div class="col-lg-5 col-md-4 col-sm-4">
                <input class="form-control" id="ciname" ng-model="chargeItemNew.projectName">
            </div>
            <div class="col-lg-6 col-md-6 col-sm-6">
                <button type="button" class="btn btn-default btn-primary" ng-click="chargeItemNewPaginator._load();"><span class="glyphicon glyphicon-search btn_icon"></span>查询 </button>
            </div>
        </div>
    </div>
    <!--搜索信息end-->
    <div class="row padding">
        <div class="col-lg-12">
            <table>
                <thead>
                <tr>
                    <th>收费项目</th>
                    <th>优先级</th>
                </tr>
                </thead>
                <tbody>
                <tr ng-repeat="item in chargeItemNewPaginator.object.chargeItemNews" ng-show="item.ciId!=null">
                    <td>{{item.ciName}}</td>
                    <td>{{item.priority}}</td>
                </tr>
                </tbody>
            </table>
        </div>
    </div>
    <div class="row padding">
        <div class="col-lg-12">
            <!--分页开始-->
            <div class="bill_paging pull-right">
                <ul class="pagination">
                    <li><a style="cursor: pointer;" ng-show="chargeItemNewPaginator.hasPrevious()" ng-click="chargeItemNewPaginator.previous()">上一页</a></li>
                    <li><a style="cursor: pointer;" ng-repeat="i in chargeItemNewPaginator.pages"  ng-click="chargeItemNewPaginator.go(i)">{{i}}</a></li>
                    <li><a style="cursor: pointer;" ng-show="chargeItemNewPaginator.hasNext()" ng-click="chargeItemNewPaginator.next()">下一页</a></li>
                </ul>
            </div>
            <!--分页结束-->
        </div>
    </div>
    <!--
    <div class="row padding">
        <div class="pull-right" style="margin-right: 15px;">
            <button class="btn btn-primary ">保存</button>
        </div>
        <div class="pull-right" style="margin-right: 15px;">
            <button class="btn btn-primary ">确认</button>
        </div>
    </div>
    -->
</div>
</body>
</html>