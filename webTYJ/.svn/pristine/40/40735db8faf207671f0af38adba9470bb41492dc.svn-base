                                    <a href="javascript:void(0)" ng-click="searchPaginator2.go(1)">首页</a>
                                </li>
                                <li ng-class="{true:'disabled'}[searchPaginator2.page.currentPage==1]">
                                    <a href="javascript:void(0)" ng-show="searchPaginator2.hasPrevious()"
                                       ng-click="searchPaginator2.previous()">上一页</a>
                                </li>
                                <li ng-class="{true:'active'}[searchPaginator2.page.currentPage==i]"
                                    ng-repeat="i in searchPaginator2.pages">
                                    <a href="javascript:void(0)" ng-click="searchPaginator2.go(i)">{{i}}</a>
                                </li>
                                <li ng-class="{true:'disabled'}[searchPaginator2.page.currentPage==searchPaginator2.page.totalPage]">
                                    <a href="javascript:void(0)" ng-show="searchPaginator2.hasNext()"
                                       ng-click="searchPaginator2.next()">下一页</a>
                                </li>
                                <li ng-class="{true:'disabled'}[searchPaginator2.page.currentPage==searchPaginator2.page.totalPage]">
                                    <a href="javascript:void(0)"
                                       ng-click="searchPaginator2.go(searchPaginator2.page.totalPage)">尾页</a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">取消</button>
                <button type="button" class="btn bg-primary" data-dismiss="modal" ng-click="addPerson1(4)">保存</button>
            </div>
        </div>
    </div>
</div>


